#include <dt-bindings/camera/exynos_is_dt.h>
#include "module_jn1.dtsi"	/* Wide CAMERA */
#include "module_3l6.dtsi"	/* Front CAMERA */
#include "module_4ha.dtsi"	/* UW CAMERA */
#include "module_gc02m2.dtsi"	/* Macro CAMERA */

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;
			interrupt-parent = <&gic>;

			/* Wide CAMERA */
			is_sensor_jn1: is_sensor_jn1@5A {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk0_out>;
				pinctrl-2 = <&sensor_mclk0_fn>;
				pinctrl-3 = <>;

				/* board */
				position = <0>; /* Rear:0. Front:1, Tele:2 */
				id = <0>; /* is_sensor id */
				mclk_ch = <0>;
				sensor_i2c_ch = <0>; /* SENSOR_CONTROL_I2C0 */

				af {
					product_name = <18>; /* ACTUATOR_NAME_AK737X : ak7371 or ak7374 or ak7377*/
					i2c_ch = <3>; /* SENSOR_CONTROL_I2C3 */
				};

				flash {
					product_name = <20>; /* SM5714 */
				};

				preprocessor {
					product_name = <100>; /* NOTHING */
				};

				mcu {
					product_name = <2>; /* INTERNAL */
				};

				power_seq {
					scenario_normal_on {
						30 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 1000 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_normal_off {
						90 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
					scenario_vision_on {
						30 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 1000 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_vision_off {
						90 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
					scenario_ois_factory_on {
						20 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 1000 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_ois_factory_off {
						40 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
					scenario_read_rom_on {
						10 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 1000 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_read_rom_off {
						20 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
				};
			};

			/* FRONT CAMERA */
			is_sensor_3l6: is_sensor_3l6@5A {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk1_out>;
				pinctrl-2 = <&sensor_mclk1_fn>;
				pinctrl-3 = <>;

				/* board */
				position = <1>;	/* Rear:0. Front:1. Rear_sub:2. Secure:3. */
				id = <1>; /* is_sensor id */
				mclk_ch = <1>;
				sensor_i2c_ch = <1>; /* SENSOR_CONTROL_I2C1 */

				power_seq {

					scenario_normal_on {
						20 { pname = "UWCAM_MACROCAM_VT_EN"; pin = <PIN_OUTPUT 1 1000>; gpio = <&gpm12 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 1000 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_normal_off {
						70 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						90 { pname = "UWCAM_MACROCAM_VT_EN"; pin = <PIN_OUTPUT 0 1000>; gpio = <&gpm12 0 0x1>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
					scenario_vision_on {
						20 { pname = "UWCAM_MACROCAM_VT_EN"; pin = <PIN_OUTPUT 1 1000>; gpio = <&gpm12 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 1000 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_vision_off {
						70 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						90 { pname = "UWCAM_MACROCAM_VT_EN"; pin = <PIN_OUTPUT 0 1000>; gpio = <&gpm12 0 0x1>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
					scenario_read_rom_on {
						10 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 1000 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_read_rom_off {
						20 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
				};
			};

			/* UW CAMERA */
			is_sensor_4ha: is_sensor_4ha@5A {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk2_out>;
				pinctrl-2 = <&sensor_mclk2_fn>;
				pinctrl-3 = <>;

				/* board */
				position = <4>;	/* Rear:0. Front:1. Rear_sub:2. Secure:3. */
				id = <4>; /* is_sensor id */
				mclk_ch = <2>;
				sensor_i2c_ch = <2>; /* SENSOR_CONTROL_I2C2 */

				power_seq {
					scenario_normal_on {
						20 { pname = "VDD_VT_UW_MACRO_A2P8"; pin = <PIN_OUTPUT 1 0 2800000>; gpio = <&gpm12 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 0 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						70 { pname = "MCLK"; pin = <PIN_MCLK 1 1500>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};
					scenario_normal_off {
						20 { pname = "MCLK"; pin = <PIN_MCLK 0 500>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>; };
						70 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						90 { pname = "VDD_VT_UW_MACRO_A2P8"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpm12 0 0x1>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
					scenario_vision_on {
						20 { pname = "VDD_VT_UW_MACRO_A2P8"; pin = <PIN_OUTPUT 1 0 2800000>; gpio = <&gpm12 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 0 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						70 { pname = "MCLK"; pin = <PIN_MCLK 1 1500>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};
					scenario_vision_off {
						20 { pname = "MCLK"; pin = <PIN_MCLK 0 500>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>; };
						70 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						90 { pname = "VDD_VT_UW_MACRO_A2P8"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpm12 0 0x1>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
					scenario_read_rom_on {
						20 { pname = "VDD_VT_UW_MACRO_A2P8"; pin = <PIN_OUTPUT 1 0 2800000>; gpio = <&gpm12 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 0 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						70 { pname = "MCLK"; pin = <PIN_MCLK 1 1500>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};
					scenario_read_rom_off {
						20 { pname = "MCLK"; pin = <PIN_MCLK 0 500>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>; };
						70 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						90 { pname = "VDD_VT_UW_MACRO_A2P8"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpm12 0 0x1>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
				};
			};

			/* MACRO CAMERA */
			is_sensor_gc02m2_macro: is_sensor_gc02m2_macro@20 {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk2_out>;
				pinctrl-2 = <&sensor_mclk2_fn>;
				pinctrl-3 = <>;

				/* board */
				position = <6>;	/* Rear:0. Front:1. Rear_sub:2. Secure:3. */
				id = <3>; /* is_sensor id */
				mclk_ch = <2>;
				sensor_i2c_ch = <2>; /* SENSOR_CONTROL_I2C2 */

				flash {
					product_name = <20>; /* SM5714 */
				};

				power_seq {
					scenario_normal_on {
						20 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 1000 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						40 { pname = "VDD_VT_UW_MACRO_A2P8"; pin = <PIN_OUTPUT 1 0>; gpio = <&gpm12 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 1500>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};
					scenario_normal_off {
						30 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						50 { pname = "VDD_VT_UW_MACRO_A2P8"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpm12 0 0x1>; share = <SRT_RELEASE SHARED_PIN1 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 500>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>; };
					};
					scenario_vision_on {
						20 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 1000 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						40 { pname = "VDD_VT_UW_MACRO_A2P8"; pin = <PIN_OUTPUT 1 0>; gpio = <&gpm12 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 1500>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};
					scenario_vision_off {
						30 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						50 { pname = "VDD_VT_UW_MACRO_A2P8"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpm12 0 0x1>; share = <SRT_RELEASE SHARED_PIN1 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 500>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>; };
					};
					scenario_read_rom_on {
						10 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 1 1000 1800000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_read_rom_off {
						20 { pname = "vdd_ldo22"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
				};
			};

			is_flash_gpio: is-flash-gpio@0 {
				compatible = "samsung,sensor-flash-sm5714";
				sensor-module = <&is_sensor_jn1>,<&is_sensor_gc02m2_macro>;
				id = <0 3>;
				led_ch = <1>;
				status = "okay";
				flash-gpio = <&gpc11 0 0x1>;
				torch-gpio = <&gpc12 0 0x1>;
			};

			/* OIS_MCU */
			ois_mcu: ois_mcu@151F0000 {
				compatible = "samsung,sensor-ois-mcu";
				reg = <0x0 0x151F0000 0x10000>, /* OIS MCU (SRAM 56KB) */
					<0x0 0x13900000 0x10000>, /* I2C: USI05_USI */
					<0x0 0x13910000 0x10000>, /* SPI: USI06_USI */
					<0x0 0x10040000 0x1000>, /* PERI setting */
					<0x0 0x15000000 0x10000>; /* CSIS */
				reg-names = "core", "i2c", "spi", "peri", "csis";
				interrupts = <0 INTREQ__OIS_FROM_CM0P IRQ_TYPE_LEVEL_HIGH>;

				id = <0>;
				status = "okay";
				mcu_ctrl_actuator = <0>;
				mcu_skip_video_fastae;

				power-domains = <&pd_csis>;
				clocks = <&clock GATE_OIS_MCU_TOP_QCH>, <&clock DOUT_DIV_CLK_PERI_USI06_USI>;
				clock-names = "user_mux", "ipclk_spi";
			};

			/* dphy_setfile settings taken from A54x */
			dphy_setfile_m35x_1: dphy-setfile_m35x_1@00 {
				comm {
					/* bias */
					0  = <0x1000 0 32 0x00000010 IDX_BIA_VAL 0>; /* M_BIAS_CON0 */
					1  = <0x1004 0 32 0x00000110 IDX_BIA_VAL 0>; /* M_BIAS_CON1 */
					2  = <0x1008 0 32 0x00003223 IDX_BIA_VAL 0>; /* M_BIAS_CON2 */
					3  = <0x1010 0 32 0x00000200 IDX_BIA_VAL 0>; /* M_BIAS_CON4 */
					4  = <0x1114 0 32 0x00000500 IDX_BIA_VAL 0>; /* M_PLL_CON5 */
					5  = <0x1118 0 32 0x00000000 IDX_BIA_VAL 0>; /* M_PLL_CON6 */
					6  = <0x111C 0 32 0x000028A0 IDX_BIA_VAL 0>; /* M_PLL_CON7 */
					7  = <0x1120 0 32 0x00001450 IDX_BIA_VAL 0>; /* M_PLL_CON8 */
					8  = <0x1124 0 32 0x00001208 IDX_BIA_VAL 0>; /* M_PLL_CON9 */

					/* Clock lane */
					9 = <0x0004 0 32 0x000028A0 IDX_FIX_VAL 0>; /* SC_GNR_CON1 */
					10 = <0x0008 0 32 0x00000005 IDX_FIX_VAL 0>; /* SC_ANA_CON0 */
					11 = <0x000C 0 32 0x0000E840 IDX_FIX_VAL 0>; /* SC_ANA_CON1 */
					12 = <0x0010 0 32 0x00000202 IDX_FIX_VAL 0>; /* SC_ANA_CON2 */
					13 = <0x0014 0 32 0x00008600 IDX_FIX_VAL 0>; /* SC_ANA_CON3 */
					14 = <0x0018 0 32 0x00004000 IDX_FIX_VAL 0>; /* SC_ANA_CON4 */
					15 = <0x0030 0 32 0x00000308 IDX_FIX_VAL 0>; /* SC_TIME_CON0 */
					16 = <0x0040 0 32 0x00000000 IDX_FIX_VAL 0>; /* SC_DATA_CON0 */

					/* Enable should be set at last */
					17 = <0x0000 0 32 0x00000001 IDX_FIX_VAL 0>; /* SC_GNR_CON0 */
				};

				lane {
					0  = <0x0004 0 32 0x000028A0 IDX_FIX_VAL 4>; /* SD_GNR_CON1 */
					1  = <0x0008 0 32 0x00000005 IDX_FIX_VAL 4>; /* SD_ANA_CON0 */
					2  = <0x000C 0 32 0x0000E840 IDX_FIX_VAL 4>; /* SD_ANA_CON1 */
					3  = <0x0010 0 32 0x00000202 IDX_FIX_VAL 4>; /* SD_ANA_CON2 */
					4  = <0x0010 8  2 0x00000000 IDX_SKW_DLY 4>; /* SD_ANA_CON2 */
					5  = <0x0014 0 32 0x00008600 IDX_FIX_VAL 4>; /* SD_ANA_CON3 */
					6  = <0x0018 0 32 0x00004000 IDX_FIX_VAL 4>; /* SD_ANA_CON4 */
					7  = <0x0030 0  8 0x00000000 IDX_STL_VAL 4>; /* SD_TIME_CON0 */
					8  = <0x0030 8  1 0x00000000 IDX_STL_CLK 4>; /* SD_TIME_CON0 */
					9  = <0x0034 0 32 0x00000003 IDX_FIX_VAL 4>; /* SD_TIME_CON1 */
					10 = <0x0040 0  1 0x00000001 IDX_SKW_CAL 4>; /* SD_DESKEW_CON0 */
					11 = <0x0050 0 32 0x0000081A IDX_FIX_VAL 4>; /* SD_DESKEW_CON4 */

					/* Enable should be set at last */
					12 = <0x0000 0 32 0x00000001 IDX_FIX_VAL 4>;/* SD_GNR_CON0 */
				};
			};

			dphy_setfile_m35x_2: dphy-setfile_m35x_2@00 {
				comm {
					/* bias */
					0  = <0x1000 0 32 0x00000010 IDX_BIA_VAL 0>; /* M_BIAS_CON0 */
					1  = <0x1004 0 32 0x00000110 IDX_BIA_VAL 0>; /* M_BIAS_CON1 */
					2  = <0x1008 0 32 0x00003223 IDX_BIA_VAL 0>; /* M_BIAS_CON2 */
					3  = <0x1010 0 32 0x00000200 IDX_BIA_VAL 0>; /* M_BIAS_CON4 */
					4  = <0x1114 0 32 0x00000500 IDX_BIA_VAL 0>; /* M_PLL_CON5 */
					5  = <0x1118 0 32 0x00000000 IDX_BIA_VAL 0>; /* M_PLL_CON6 */
					6  = <0x111C 0 32 0x000028A0 IDX_BIA_VAL 0>; /* M_PLL_CON7 */
					7  = <0x1120 0 32 0x00001450 IDX_BIA_VAL 0>; /* M_PLL_CON8 */
					8  = <0x1124 0 32 0x00001208 IDX_BIA_VAL 0>; /* M_PLL_CON9 */

					/* Clock lane */
					9 = <0x0004 0 32 0x000028A0 IDX_FIX_VAL 0>; /* SC_GNR_CON1 */
					10 = <0x0008 0 32 0x00000001 IDX_FIX_VAL 0>; /* SC_ANA_CON0 */
					11 = <0x000C 0 32 0x0000E840 IDX_FIX_VAL 0>; /* SC_ANA_CON1 */
					12 = <0x0010 0 32 0x00000002 IDX_FIX_VAL 0>; /* SC_ANA_CON2 */
					13 = <0x0014 0 32 0x00008600 IDX_FIX_VAL 0>; /* SC_ANA_CON3 */
					14 = <0x0018 0 32 0x00004000 IDX_FIX_VAL 0>; /* SC_ANA_CON4 */
					15 = <0x0030 0 32 0x00000308 IDX_FIX_VAL 0>; /* SC_TIME_CON0 */
					16 = <0x0040 0 32 0x00000000 IDX_FIX_VAL 0>; /* SC_DATA_CON0 */

					/* Enable should be set at last */
					17 = <0x0000 0 32 0x00000001 IDX_FIX_VAL 0>; /* SC_GNR_CON0 */
				};
			};

			dphy_setfile_m54x_4ha: dphy-setfile_m54x_4ha@00 {
				comm {
					/* bias */
					0  = <0x1000 0 32 0x00000010 IDX_BIA_VAL 0>; /* M_BIAS_CON0 */
					1  = <0x1004 0 32 0x00000110 IDX_BIA_VAL 0>; /* M_BIAS_CON1 */
					2  = <0x1008 0 32 0x00003223 IDX_BIA_VAL 0>; /* M_BIAS_CON2 */
					3  = <0x1010 0 32 0x00000200 IDX_BIA_VAL 0>; /* M_BIAS_CON4 */
					4  = <0x1114 0 32 0x00000500 IDX_BIA_VAL 0>; /* M_PLL_CON5 */
					5  = <0x1118 0 32 0x00000000 IDX_BIA_VAL 0>; /* M_PLL_CON6 */
					6  = <0x111C 0 32 0x000028A0 IDX_BIA_VAL 0>; /* M_PLL_CON7 */
					7  = <0x1120 0 32 0x00001450 IDX_BIA_VAL 0>; /* M_PLL_CON8 */
					8  = <0x1124 0 32 0x00001208 IDX_BIA_VAL 0>; /* M_PLL_CON9 */

					/* Clock lane */
					9 = <0x0004 0 32 0x000028A0 IDX_FIX_VAL 0>; /* SC_GNR_CON1 */
					10 = <0x0008 0 32 0x00000001 IDX_FIX_VAL 0>; /* SC_ANA_CON0 */
					11 = <0x000C 0 32 0x0000E840 IDX_FIX_VAL 0>; /* SC_ANA_CON1 */
					12 = <0x0010 0 32 0x00000002 IDX_FIX_VAL 0>; /* SC_ANA_CON2 */
					13 = <0x0014 0 32 0x00008600 IDX_FIX_VAL 0>; /* SC_ANA_CON3 */
					14 = <0x0018 0 32 0x00004000 IDX_FIX_VAL 0>; /* SC_ANA_CON4 */
					15 = <0x0030 0 32 0x00000308 IDX_FIX_VAL 0>; /* SC_TIME_CON0 */
					16 = <0x0040 0 32 0x00000000 IDX_FIX_VAL 0>; /* SC_DATA_CON0 */

					/* Enable should be set at last */
					17 = <0x0000 0 32 0x00000001 IDX_FIX_VAL 0>; /* SC_GNR_CON0 */
				};

				lane {
					0  = <0x0004 0 32 0x000028A0 IDX_FIX_VAL 4>; /* SD_GNR_CON1 */
					1  = <0x0008 0 32 0x00000001 IDX_FIX_VAL 4>; /* SD_ANA_CON0 */
					2  = <0x000C 0 32 0x0000E840 IDX_FIX_VAL 4>; /* SD_ANA_CON1 */
					3  = <0x0010 0 32 0x00000002 IDX_FIX_VAL 4>; /* SD_ANA_CON2 */
					4  = <0x0010 8  2 0x00000000 IDX_SKW_DLY 4>; /* SD_ANA_CON2 */
					5  = <0x0014 0 32 0x00008600 IDX_FIX_VAL 4>; /* SD_ANA_CON3 */
					6  = <0x0018 0 32 0x00004000 IDX_FIX_VAL 4>; /* SD_ANA_CON4 */
					7  = <0x0030 0  8 0x00000000 IDX_STL_VAL 4>; /* SD_TIME_CON0 */
					8  = <0x0030 8  1 0x00000000 IDX_STL_CLK 4>; /* SD_TIME_CON0 */
					9  = <0x0034 0 32 0x00000003 IDX_FIX_VAL 4>; /* SD_TIME_CON1 */
					10 = <0x0040 0  1 0x00000001 IDX_SKW_CAL 4>; /* SD_DESKEW_CON0 */
					11 = <0x0050 0 32 0x0000081A IDX_FIX_VAL 4>; /* SD_DESKEW_CON4 */

					/* Enable should be set at last */
					12 = <0x0000 0 32 0x00000001 IDX_FIX_VAL 4>;/* SD_GNR_CON0 */
				};
			};

			/* Wide CAMERA */
			is_sensor0: is_sensor0@15030000 {
				#address-cells = <1>;
				#size-cells = <1>;
				scenario = <SENSOR_SCENARIO_NORMAL>;	/* Normal, Vision, OIS etc */
				id = <0>;
				csi_ch = <0>;
				status = "okay";
				use_cphy = <0>;

				interrupts = <0 INTREQ__CSIS0 IRQ_TYPE_LEVEL_HIGH>; /*MIPI-CSI0 */
				interrupt-names = "csi";
				phys = <&mipi_phy_csis0_m0s4s4s4s4_s22 0>;
				phy_setfile = <&dphy_setfile>;
				phy_tune_default_mode = "4080 3060 30";
			};

			/* FRONT CAMERA */
			is_sensor1: is_sensor1@15040000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x15050000 0x1000>, /* MIPI-CSI2 */
