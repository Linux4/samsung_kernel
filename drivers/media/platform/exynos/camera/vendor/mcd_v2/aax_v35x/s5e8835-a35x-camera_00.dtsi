#include <dt-bindings/camera/exynos_is_dt.h>
#include "module_gn8.dtsi"	/* Wide CAMERA */
#include "module_imx882.dtsi"	/* Dualized Wide CAMERA */
#include "module_3l6.dtsi"	/* Front CAMERA */
#include "module_gc08a3.dtsi"	/* UW CAMERA */
#include "module_gc05a3.dtsi"	/* Macro CAMERA */
#include "module_hi1339.dtsi"	/* FRONT CAMERA */

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;
			interrupt-parent = <&gic>;

			/* Wide CAMERA */
			is_sensor_gn8: is_sensor_gn8@7A {
				preload_setfile;
				pinning_setfile;

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk0_out>;
				pinctrl-2 = <&sensor_mclk0_fn>;
				pinctrl-3 = <>;

				/* board */
				position = <0>; /* Rear:0. Front:1, Tele:2 */
				id = <0>; /* is_sensor id */
				mclk_ch = <0>;
				sensor_i2c_ch = <0>; /* SENSOR_CONTROL_I2C0 */

				flash {
					product_name = <20>; /* SM5714 */
				};

				af {
					product_name = <18>; /* ACTUATOR_NAME_AK737X : ak7371 or ak7374 or ak7377*/
					i2c_ch = <3>; /* SENSOR_CONTROL_I2C3 */
				};

				mcu {
					product_name = <2>; /* INTERNAL */
				};

				power_seq {
					scenario_normal_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_normal_off {
						100 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
					scenario_vision_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_vision_off {
						100 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
					scenario_ois_factory_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_ois_factory_off {
						100 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
					scenario_read_rom_on {
						10 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_read_rom_off {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 5000>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};

				};
			};

			is_sensor_imx882: is_sensor_imx882@66 {
				preload_setfile;
				pinning_setfile;

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk0_out>;
				pinctrl-2 = <&sensor_mclk0_fn>;
				pinctrl-3 = <>;

				/* board */
				position = <0>; /* Rear:0. Front:1, Tele:2 */
				id = <0>; /* is_sensor id */
				mclk_ch = <0>;
				sensor_i2c_ch = <0>; /* SENSOR_CONTROL_I2C0 */

				flash {
					product_name = <20>; /* SM5714 */
				};

				af {
					product_name = <18>; /* ACTUATOR_NAME_AK737X : ak7371 or ak7374 or ak7377*/
					i2c_ch = <3>; /* SENSOR_CONTROL_I2C3 */
				};

				mcu {
					product_name = <2>; /* INTERNAL */
				};

				power_seq {

					scenario_normal_on {
						40 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_normal_off {
						60 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
					scenario_vision_on {
						40 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_vision_off {
						60 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
					scenario_ois_factory_on {
						40 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
