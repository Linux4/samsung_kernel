					};
					scenario_vision_on {
						20 { pname = "VT_RACM3_RCAM4_LDO_EN"; pin = <PIN_OUTPUT 1 1000>; gpio = <&gpm12 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 1750>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>;};
					};
