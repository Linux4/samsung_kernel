				pinctrl-3 = <>;

				/* board */
				position = <1>;	/* Rear:0. Front:1. Rear_sub:2. Secure:3. */
				id = <1>; /* is_sensor id */
				mclk_ch = <2>;
				sensor_i2c_ch = <1>; /* SENSOR_CONTROL_I2C1 */

				power_seq {

					scenario_normal_on {
						20 { pname = "VT_RACM3_RCAM4_LDO_EN"; pin = <PIN_OUTPUT 1 1000>; gpio = <&gpm12 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 1750>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};
					scenario_normal_off {
						20 { pname = "MCLK"; pin = <PIN_MCLK 0 500>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>; };
						70 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						90 { pname = "VT_RACM3_RCAM4_LDO_EN"; pin = <PIN_OUTPUT 0 1000>; gpio = <&gpm12 0 0x1>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
					scenario_vision_on {
						20 { pname = "VT_RACM3_RCAM4_LDO_EN"; pin = <PIN_OUTPUT 1 1000>; gpio = <&gpm12 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 1750>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};
					scenario_vision_off {
						20 { pname = "MCLK"; pin = <PIN_MCLK 0 500>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>; };
						70 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 500>; share = <SRT_RELEASE SHARED_PIN0 0>; };
