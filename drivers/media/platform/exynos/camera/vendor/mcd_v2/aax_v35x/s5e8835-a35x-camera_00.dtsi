						10 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_read_rom_off {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 5000>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
				};
			};

			/* FRONT CAMERA */
			is_sensor_3l6: is_sensor_3l6@5A {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk2_out>;
