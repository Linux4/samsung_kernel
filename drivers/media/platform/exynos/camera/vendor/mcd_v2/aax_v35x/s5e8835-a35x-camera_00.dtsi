					};
					scenario_read_rom_on {
						10 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};
					scenario_read_rom_off {
