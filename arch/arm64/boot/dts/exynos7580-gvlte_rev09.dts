			samsung,pin-pud-pdn = <1>;
		};

		uart0_cts: uart0-cts {
			samsung,pins = "gpb0-6";
			samsung,pin-function = <2>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <1>;
		};

		uart0_tx: uart0-tx {
