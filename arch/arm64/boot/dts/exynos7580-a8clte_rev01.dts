/dts-v1/;
#include "exynos7580-reserve-2000MB.dtsi"
#include "exynos7580.dtsi"
#include "modem-carmen2-pdata.dtsi"
#include "exynos7580-a8clte_battery_01.dtsi"
#include "exynos7580-a8clte_gpio_01.dtsi"
#include "exynos7580-a8clte_fingerprint-sensor_00.dtsi"

/ {
	model = "Samsung a8c revision 01 board based on EXYNOS7580";
	model_info-chip = <7580>;
	model_info-platform = "android";
	model_info-subtype = "samsung";
	model_info-hw_rev = <1>;
	model_info-hw_rev_end = <255>;
	compatible = "samsung,exynos7580-universal7580,r00", "samsung,exynos7580";

	chosen {
		bootargs = "console=ttySAC1,115200n8 ess_setup=0x46000000 pmic_info=0x02";
		linux,initrd-start = <0x42000000>;
		linux,initrd-end = <0x421FFFFF>;
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos7580-oscclk";
			clock-frequency = <26000000>;
		};
	};

	dwmmc0@13540000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		use-smu;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		only_once_tune;
		use-fine-tuning;
		ignore-phase = <(1 << 7)>;
		bypass-for-allpass;
		supports-highspeed;
		supports-hs200-mode;
		supports-ddr200-enhanced-strobe;
		supports-poweroff-notification;
		supports-ddr200-mode;
		enable-cache-control;
		use-enable-shift;
		enable-ulp-mode;
		desc-sz = <4>;
		qos_int_level = <200000>;
		fifo-depth = <0x40>;
		card-detect-delay = <100>;
		data-timeout = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr-timing = <3 0 2 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr200-timing = <1 0 2 0>;
		samsung,dw-mshc-ddr200-ulp-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr200-delay-line = <0x28>;
		samsung,dw-mshc-ddr200-tx-delay-line = <0x28>;
		samsung,dw-mshc-txdt-crc-timer-fastlimit = <0x34>;
		samsung,dw-mshc-txdt-crc-timer-initval = <0x38>;
		clk_pin = "gpr0-0";
		clk_addr = "13750000.pinctrl";
		clk_val = <0x3>;
		clk_str_num = <6>;
		num-ref-clks = <10>;
		ciu_clkin = <25 50 50 100 200 100 200 400 400 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@13550000 {
		status = "okay";
		num-slots = <1>;
		channel = <1>;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		/* broken-cd; */
		caps-control;
		supports-highspeed;
		supports-sdr104-mode;
		keep-power-in-suspend;
		pm-ignore-notify;
		cd-type-external;
		use-broken-voltage;
		use-fine-tuning;
		ignore-phase = <(1 << 7)>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <468000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 2 1>;
		clk_pin = "gpr2-0";
		clk_addr = "13750000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		tp_mon_depth = <2>;
				/* Byte/s,  MIF clk, CPU clk, KFC clk*/
                tp_mon_table = <8000000 825000  800000  0
				0       0       0       0>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;
		slot@0 {
			reg = <0>;
			bus-width = <4>;
		};
	};

	dwmmc2@13560000 {
		status = "okay";
		num-slots = <1>;
		supports-highspeed;
		enable-cclk-on-suspend;
		supports-sdr104-mode;
		bypass-smu;
		use-fine-tuning;
		clock-gate;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		data-timeout = <200>;
		qos_int_level = <468000>;
		ignore-phase = <(1 << 7)>;
		bypass-for-allpass;
		fix-fmp-size-mismatch;
		not-allow-single-dma;
		desc-sz = <4>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr-timing = <3 0 2 1>;
		clk_pin = "gpr4-0";
		clk_addr = "13750000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default", "output";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq>;
		pinctrl-1 = <&sd2_clk_output &sd2_cmd_output &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq>;
		gpio-controller;
		sec-sd-slot-type = <1>; /* Normal SD slot */
		#gpio-cells = <2>;
		cd-gpio = <&gpa3 1 0xf>;
		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	gpio_keys{
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&gpa2_pull_up_univ>;

		button@1 {
			label = "gpio-keys: KEY_POWER";
			interrupt-parent = <&gpa2>;
			linux,code = <116>;
			gpios = <&gpa2 7 0xf>;
			gpio-key,wakeup = <1>;
		};
		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			interrupt-parent = <&gpa2>;
			linux,code = <114>;
			gpios = <&gpa2 1 0xf>;
		};
		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			interrupt-parent = <&gpa2>;
			linux,code = <115>;
			gpios = <&gpa2 0 0xf>;
		};
		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			interrupt-parent = <&gpa2>;
			linux,code = <172>;
			gpios = <&gpa2 5 0xf>;
			gpio-key,wakeup = <1>;
		};
	};

	/* TSP_SCL / TSP_SDA */
	pinctrl@138D0000 {
		i2c_pdn_off: i2c-pdn_off {
			samsung,pins = "gpc4-1", "gpc4-0";
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};
	};

	pinctrl@139B0000 {
		pmic_irq: pmic-irq {
			samsung,pins = "gpa0-2";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		/* TOUCH_INT */
		cfg_irq: cfg-irq {
			samsung,pins = "gpa0-3";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		cfg_input: cfg-input {
			samsung,pins = "gpa0-3";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		/* PMIC_IRQBC */
		cod3022_irq: cod3022-irq {
			samsung,pins = "gpa0-4";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		/* MUIC_INT */
		muic_irq: muic-irq {
			samsung,pins = "gpa1-3";
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		prox_irq: prox_irq {
			samsung,pins = "gpa0-7";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		acc_int: acc_int {
			samsung,pins = "gpa3-0";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		attn_input: attn-input {
			samsung,pins = "gpa3-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		attn_irq: attn-irq {
			samsung,pins = "gpa3-2";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		/* T_FLASH_DET */
		dwmmc2_cd_ext_irq: dwmmc2_cd_ext_irq {
			samsung,pins = "gpa3-1";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		earjack_jack_det: jack-det {
			samsung,pins = "gpa3-1";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		earjack_key_det: key-det {
			samsung,pins = "gpa1-1";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@139C0000 {
		/* Vibrator Ctrl */
		motor_en: motor_en {
			samsung,pins = "gpe0-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		vibetonz_pwm: vibetonz_pwm {
			samsung,pins = "gpc1-1";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		/* TOUCH_SCL / SDA */
		cfg_i2c: cfg-i2c {
			samsung,pins = "gpc5-3", "gpc5-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		cfg_gpio: cfg-gpio {
			samsung,pins = "gpc5-3", "gpc5-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		/* need check */
		main_mic_bias_en:main-mic-bias-en {
			samsung,pins ="gpd2-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
			samsung,pin-val = <0>;
		};
		/* Warm reset information from AP */
		pm_wrsti: pm-wrsti {
			samsung,pins = "gpe1-0";
			samsung,pin-con-pdn = <3>;
		};
	};

	spi_0: spi@13910000 {
		/delete-property/ dma-mode;
		/delete-property/ dmas;
		/delete-property/ dma-names;
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&spi0_bus>;
		pinctrl-1 = <&spi0_cs>;
		num-cs = <1>;
		status = "okay";
	};

	spi_1: spi@13920000 {
		/delete-property/ dma-mode;
		/delete-property/ dmas;
		/delete-property/ dma-names;
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&spi1_bus>;
		pinctrl-1 = <&spi1_cs>;
		status = "okay";
	};

	i2c@13830000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;
		supports-bus-clear;

		s2801x: s2801x@7a {
			compatible = "samsung,s2801x";
			reg = <0x7a>;
			clocks = <&clock 154>,	/* sclk_audmixer */
				 <&clock 155>,	/* sclk_audmixer_bclk0*/
				 <&clock 156>,	/* sclk_audmixer_bclk1 */
				 <&clock 157>,	/* sclk_audmixer_bclk2 */
				 <&clock 152>,	/* bclk_mi2s */
				 <&clock 149>;	/* dout_audmixer*/

			clock-names = "audmixer_sysclk", "audmixer_bclk0",
					"audmixer_bclk1", "audmixer_bclk2",
					"mi2s_bclk", "audmixer_dout";
			sysreg-reset = <0x110F2000>;
			sysreg-i2c = <0x110F3000>;
			pinctrl-names = "default", "idle";
			pinctrl-0 = <&i2s_pmic_bus
					&i2s2_codec_clk
					&pcm_bt_bus
					&pcm_bt1_bus>;
			pinctrl-1 = <&i2s_pmic_bus_idle
					&i2s2_codec_clk_idle
					&pcm_bt_bus_idle
					&pcm_bt1_bus_idle>;
			samsung,lpass-subip;
		};
	};

	i2c@13860000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;

		mms_ts@48 {
			compatible = "melfas,mms_ts";
			reg = <0x48>;
			pinctrl-names = "on_state", "off_state";
			pinctrl-0 = <&attn_irq>;
			pinctrl-1 = <&attn_input &i2c_pdn_off>;
			melfas,vdd_en = "tsp_avdd";
			melfas,io_en = "vdd_ldo3";
			melfas,irq-gpio = <&gpa3 2 0>;
			melfas,scl-gpio = <&gpc4 1 0>;
			melfas,sda-gpio = <&gpc4 0 0>;
			melfas,max_x = <2160>;
			melfas,max_y = <3840>;
			melfas,fw_name = "tsp_melfas/mms449_a8.fw";
		};
	};

	/* Touchkey */
	i2c@13840000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <200000>;
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-0 = <&i2c7_bus>;

		touchkey@20 {
			pinctrl-names = "on_irq","off_irq";
			pinctrl-0 = <&cfg_irq>;
			pinctrl-1 = <&cfg_input>;
			compatible = "abov,mc96ft16xx";
			reg = <0x20>;
			interrupts = <3 0 0>;
			interrupt-parent = <&gpa0>;
			abov,sda-gpio = <&gpc5 3 0>;
			abov,scl-gpio = <&gpc5 2 0>;
			abov,irq-gpio = <&gpa0 3 0>;
			abov,regulator_ic = "vtouch_2.8v";
			abov,regulator_led = "vtouch_led_2.8v";
			abov,boot-on-ldo = "true";
		};
	};

	/* MUIC */
	i2c@25 {
		compatible = "i2c-gpio";
		gpios = <&gpd3 4 0
			&gpd3 3 0>;
		i2c-gpio,delay-us = <5>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		muic-universal@25 {
			status = "okay";
			compatible = "muic-universal";
			reg = <0x25>;
			pinctrl-names = "default";
			pinctrl-0 = <&muic_irq>;
			muic-universal,chip_name = "sm,sm5703";
			muic-universal,irq-gpio = <&gpa1 3 0>;
			muic-universal,uart-gpio = <&gpd2 2 0>;
			muic-universal,uart-ap = <0x1>;
			regulators {
				ESAFEOUT1 {
					regulator-compatible = "safeout1";
					regulator-name = "safeout1_range";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <4900000>;
					regulator-boot-on;
				};

				ESAFEOUT2 {
					regulator-compatible = "safeout2";
					regulator-name = "safeout2_range";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <4900000>;
				};
			};
		};
	};

	muic {
		status = "disabled";
		muic,support-list = "+OTG:GND",
					"-MHL:1K",
					"-VZW Accessory:28.7K",
					"-VZW Incompatible:34K",
					"-Smartdock:40.2K",
					"-HMT:49.9K",
					"-Audiodock:64.9K",
					"-USB LANHUB:80.07K",
					"+Charging Cable:102K",
					"-Universal Multimedia dock:121K",
					"+Jig USB Off:255K",
					"+Jig USB On:301K",
					"-Deskdock:365K",
					"+TYPE2 Charger:442K",
					"+Jig UART Off:523K",
					"+Jig UART On:619K",
					"+TA:OPEN",
					"+USB:OPEN",
					"+CDP:OPEN",
					"+Undefined Charging:XXX";
	};

	pinctrl@139C0000 {
		fled_flash_sleep: fled-flash-sleep {
			samsung,pins = "gpe0-2", "gpe0-1";
			samsung,pin-function = <2>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		fled_flash_active: fled-flash-active {
			samsung,pins = "gpe0-2", "gpe0-1";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
	};

	hsi2c@13880000 {
		status = "okay";
		clock-frequency = <400000>;
		fimc_is_eeprom_i2c@51{
			compatible = "samsung,front-eeprom-i2c";
			reg = <0x51>;
			use_i2c_pinctrl;
			fimc_is_front_eeprom_pinname = "139C0000.pinctrl";
			fimc_is_front_eeprom_sda = "gpc2-2";
			fimc_is_front_eeprom_scl = "gpc2-3";
			pinfunc_on = <4>;
			pinfunc_off = <0>;
		};
		fimc_is_eeprom_i2c@58 {
			compatible = "samsung,rear-eeprom-i2c";
			reg = <0x58>;
			use_i2c_pinctrl;
			fimc_is_rear_eeprom_pinname = "139C0000.pinctrl";
			fimc_is_rear_eeprom_sda = "gpc2-2";
			fimc_is_rear_eeprom_scl = "gpc2-3";
			pinfunc_on = <4>;
			pinfunc_off = <0>;
		};
	};

	hsi2c@104c0000 {
		status = "okay";
		samsung,hs-mode;
		clock-frequency = <2500000>;
		samsung,hsi2c-batcher;
		#address-cells = <1>;
		#size-cells = <0>;

		audio_codec_cod3022x: cod3022x@04 {
			compatible = "codec,cod3022x";
			reg = <0x04>;
			ten-bit-address;
			vdd1-supply = <&ldo22_reg>;
			vdd2-supply = <&ldo23_reg>;
			pinctrl-names = "default";
			pinctrl-0 = <&cod3022_irq>;
			interrupt-parent = <&gpa0>;
			gpios = <&gpa0 4 0xf>;
			/* Mic Bias: 0->2.1v, 1->2.5v, 2->2.6v, 3->3.0v */
			mic-bias1-voltage = <3>;
			mic-bias2-voltage = <3>;
		};

		s2mpu03_pmic@66 {
			compatible = "samsung,s2mpu03-pmic";
			reg = <0x66>;
			ten-bit-address;
			interrupts = <2 0 0>;
			interrupt-parent = <&gpa0>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_irq &pm_wrsti>;
			g3d_en = <1>; /* 1 : enable , 0 : disable */
			/*
			buck4en_pin = "etc1-3";
			buck4en_addr = "1111111.pinctrl";
			*/
			/* RTC: wtsr/smpl */
			wtsr_en = <1>; /* enable */
			smpl_en = <1>; /* enable */
			wtsr_timer_val = <3>;   /* 1000ms */
			smpl_timer_val = <4>;   /* 500ms */
			check_jigon = <0>; /* do not check jigon */
			/* RTC: If it's first boot, reset rtc to 1/1/2015 12:00:00(Thu) */
			init_time,sec = <0>;
			init_time,min = <0>;
			init_time,hour = <12>;
			init_time,mday = <1>;
			init_time,mon = <0>;
			init_time,year = <115>;
			init_time,wday = <4>;

			regulators {
				buck1_reg: BUCK1 {
					regulator-name = "cpu-cluster.1";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1400000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck2_reg: BUCK2 {
					regulator-name = "cpu-cluster.0";
