#include "exynos7570-rev01.dtsi"

/ {
	model = "Samsung J2COREPLTE USA AIO board based on EXYNOS7570";
	compatible = "samsung,exynos7570", "samsung,exynos4210";

	memory@40000000 {
		device_type = "memory";
		reg = <0x0 0x40000000 0x3E800000>;
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x0 0x80000000 0x40000000>;
	};

	chosen {
		bootargs = "console=ttySAC2,115200 clk_ignore_unused androidboot.hardware=samsungexynos7570 androidboot.selinux=disabled firmware_class.path=/vendor/firmware ess_setup=0x46000000 bcm_setup=0xffffff80f8e00000 androidboot.debug_level=0x4948 reserve-fimc=0";
		linux,initrd-start = <0x42000000>;
		linux,initrd-end = <0x421FFFFF>;
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos7570-oscclk";
			clock-frequency = <26000000>;
		};
	};

	serial_2: uart@13820000 {
		samsung,uart-logging;
		status = "okay";
	};

	firmware {
		android {
			compatible = "android,firmware";
			fstab {
				compatible = "android,fstab";
				system {
					compatible = "android,system";
					dev = "/dev/block/platform/13540000.dwmmc0/by-name/SYSTEM";
					type = "ext4";
					mnt_flags = "ro";
					fsmgr_flags = "wait,verify";
				};
				vendor {
					compatible = "android,vendor";
					dev = "/dev/block/platform/13540000.dwmmc0/by-name/VENDOR";
					type = "ext4";
					mnt_flags = "ro";
					fsmgr_flags = "wait,verify";
				};
				odm {
					compatible = "android,odm";
					dev = "/dev/block/platform/13540000.dwmmc0/by-name/ODM";
					type = "ext4";
					mnt_flags = "ro";
					fsmgr_flags = "wait,verify";
				};
			};
		};
	};

	dwmmc0@13540000 {
		status = "okay";
		num-slots = <1>;
		broken-card-detect;
		use-smu;
		fixed_voltage;
		supports-highspeed;
		clock-gate;
		mmc-ddr-1_8v;
		mmc-hs200-1_8v;
		mmc-hs400-1_8v;
		supports-8bit;
		supports-cmd23;
		supports-erase;
		enable-ulp-mode;
		supports-hs400-enhanced-strobe;
		pm-skip-mmc-resume-init;
		mmc_pwr_shut_down;
		card-init-hwacg-ctrl;
		fifo-depth = <0x40>;
		non-removable;
		desc-size = <4>;
		card-detect-delay = <200>;
		data-timeout = <200>;
		hto-timeout = <80>;
		clock-frequency = <700000000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-txdt-crc-timer-fastlimit = <0x34>;
		samsung,dw-mshc-txdt-crc-timer-initval = <0x38>;
		samsung,dw-mshc-ddr200-delay-line = <0x60>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr200-timing = <1 0 4 0>;
		samsung,dw-mshc-ddr200-ulp-timing = <3 0 3 0>;

		num-ref-clks = <12>;
		ciu_clkin = <25 43 43 25 43 87 175 43 43 175 175 175>;

		/* Swapping clock drive strength */
		clk-drive-number = <4>;
		pinctrl-names = "default",
			"fast-slew-rate-1x",
			"fast-slew-rate-2x",
			"fast-slew-rate-3x",
			"fast-slew-rate-4x";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		pinctrl-1 = <&sd0_clk_fast_slew_rate_1x>;
		pinctrl-2 = <&sd0_clk_fast_slew_rate_2x>;
		pinctrl-3 = <&sd0_clk_fast_slew_rate_3x>;
		pinctrl-4 = <&sd0_clk_fast_slew_rate_4x>;
		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc2@13560000 {
		status = "okay";
		num-slots = <1>;
		clock-gate;
		supports-highspeed;
		supports-4bit;
		supports-cmd23;
		supports-erase;
		sd-uhs-sdr50;
		sd-uhs-sdr104;
		card-detect-gpio;
		bypass-smu;
		only_once_tune;
		bypass-for-allpass;
		use-fine-tuning;
		skip-init-mmc-scan;
		card-init-hwacg-ctrl;
		switch_transfer;
		ignore-phase = <(1 << 7)>;
		fifo-depth = <0x40>;
		desc-size = <4>;
		card-detect-delay = <200>;
		data-timeout = <200>;
		hto-timeout = <80>;
		clock-frequency = <700000000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr-timing = <3 0 2 1>;
		samsung,dw-mshc-sdr50-timing = <3 0 4 2>;
		samsung,dw-mshc-sdr104-timing = <3 0 3 0>;

		num-ref-clks = <9>;
		ciu_clkin = <25 43 43 25 43 87 175 43 43>;

		clk-drive-number = <4>;
		pinctrl-names = "default",
			"fast-slew-rate-1x",
			"fast-slew-rate-2x",
			"fast-slew-rate-3x",
			"fast-slew-rate-4x";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq>;
		pinctrl-1 = <&sd2_clk_fast_slew_rate_1x>;
		pinctrl-2 = <&sd2_clk_fast_slew_rate_2x>;
		pinctrl-3 = <&sd2_clk_fast_slew_rate_3x>;
		pinctrl-4 = <&sd2_clk_fast_slew_rate_4x>;

		card-detect = <&gpa0 1 0xf>;
		sec-sd-slot-type = <1>; /* Hotplug SD slot */
		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	ion {
		compatible = "samsung,exynos5430-ion";
	};

	pinctrl@139C0000 {
		i2c2_bus: i2c2-bus {
			samsung,pins = "gpc2-1", "gpc2-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	i2c_2: i2c@13890000 {
		status = "okay";
		samsung,i2c-max-bus-freq = <400000>;

		lm3632@11 {
			compatible = "i2c,lm3632";
			reg = <0x11>;
		};
	};

	decon_fb@14830000 {
		psr_mode = <0>; /* 0: video mode, 1: DP command mode, 2: MIPI command mode */
		/* vclk_mif = 200MHz */
		mif-vclk = <200000000>;
		/* vclk =  200MHz */
		disp-vclk = <200000000>;

		/* Target vclk 139.333Mhz */
		/* decon_divider_num = 1024 */
		vclk-num = <1024>;
		/* decon_divider_denom = 713 */
		vclk-denom = <713>;

		/* aclk = 208Mhz */
		disp-dvfs = <208000000>;
	};

	dsim_0: dsim@14800000 {
		lcd_info = <&td4101>;
		decon_board = <&decon_board>;
		status = "okay";

		/* number of data lanes in use */
		data_lane_cnt = <2>;
	};

	decon_board: decon_board {
		gpio_blic_on = <&gpc7 4 0x1>;
		gpio_panel_enp = <&gpa2 0 0x1>;
		gpio_panel_enn = <&gpa2 1 0x1>;
		gpio_lcd_rst = <&gpd2 0 0x1>;

		dsim_set_panel_power_early {
			type =
			"regulator,enable",	"LCD_1P8",
			"delay,usleep",	"10000 11000",
			"gpio,high",	"gpio_blic_on",
			"delay,usleep",	"5000 6000";
		};
		dsim_set_panel_power_enable {
			type =
			"delay,usleep",	"1000 2000",
			"gpio,high",	"gpio_panel_enp",
			"delay,usleep",	"1000 2000",
			"gpio,high",	"gpio_panel_enn",
			"delay,usleep",	"10000 11000";
		};
		dsim_set_panel_power_disable {
			type =
			"delay,usleep",	"5000 6000",
			"gpio,low",	"gpio_panel_enn",
			"delay,usleep",	"5000 6000",
			"gpio,low",	"gpio_panel_enp",
			"delay,usleep",	"15000 16000",
			"gpio,low",	"gpio_lcd_rst",
			"delay,usleep",	"5000 6000",
			"gpio,low",	"gpio_blic_on",
			"delay,usleep",	"10000 11000",
			"regulator,disable", "LCD_1P8";
		};
		dsim_reset_panel {
			type =
			"gpio,high",	"gpio_lcd_rst",
			"delay,usleep",	"1000 1100",
			"gpio,low",	"gpio_lcd_rst",
			"delay,usleep",	"1000 1100",
			"gpio,high",	"gpio_lcd_rst",
			"delay,msleep",	"130";
		};
	};

	td4101: td4101_qhd {
		mode = <0>; /* 0: video mode, 1: DP command mode, 2: MIPI command mode */
		panel_name = "td4101";
		resolution = <540 960>;
		dispif_resol = <540 960>;
		size = <62 110>;
		ddi_type = <0>;
		timing,refresh = <60>;

		/* dsim H porch : hbp, hfp, hsa */
		timing,dsim_h-porch = <40 364 4>;
		/* dsim V porch : VBP, VFP, VSA */
		timing,dsim_v-porch = <13 244 2>;

		/* decon H porch : hbp, hfp, hsa */
		timing,decon_h-porch = <40 1321 4>;
		/* decon V porch : VBP, VFP, VSA */
		timing,decon_v-porch = <13 244 2>;

		timing,dsi-hs-clk = <836>;
		timing,pms = <3 193 1>;
		timing,dsi-escape-clk = <16>;
		clklane_onoff = <1>;
	};

	usb@13600000 {
		status = "okay";

		dwc3 {
			dr_mode = "otg";
		};
	};

	usbphy@135c0000 {
		status = "okay";
		hs_tune_info = <&usb_hs_tune>;
		ss_tune_info = <&usb_ss_tune>;
	};

	usb_hs_tune:hs_tune {
		status = "okay";
		/* value = <device host> */
		tx_vref = <0x3 0x3>;
		tx_pre_emp = <0x0 0x0>;
		tx_pre_emp_puls = <0x0 0x0>;
		tx_res	= <0x2 0x2>;
		tx_rise = <0x1 0x1>;
		tx_hsxv = <0x0 0x0>;
		tx_fsls = <0x3 0x3>;
		rx_sqrx = <0x5 0x5>;
		compdis	= <0x3 0x3>;
		otg= <0x2 0x2>;
		/* true : 1, false: 0 */
		/* <enable_user_imp user_imp_value> */
		enable_user_imp = <0 0>;
		/* PHY clk : 1 , FREE clk : 0 */
		is_phyclock = <1>;
	};

	usb_ss_tune:ss_tune {
		status = "disabled";
		/* value = <device host> */
		tx_boost_level = <0x4 0x4>;
		tx_swing_level = <0x1 0x1>;
		tx_swing_full = <0x7F 0x7F>;
		tx_swing_low = <0x7F 0x7F>;
		tx_deemphasis_mode = <0x1 0x1>;
		tx_deemphasis_3p5db = <0x18 0x18>;
		tx_deemphasis_6db = <0x18 0x18>;
		enable_ssc = <0x1 0x1>;
		ssc_range = <0x0 0x1>;
		los_bias = <0x5 0x5>;
		los_mask_val = <0x104 0x104>;
		enable_fixed_rxeq_mode = <0x0 0x0>;
		fix_rxeq_value = <0x4 0x4>;
		set_crport_mpll_charge_pump = <0x0 0x0>;
		set_crport_level_en = <0x0 0x0>;
	};

	usb_notifier {
		compatible = "samsung,usb-notifier";
		udc = <&udc>;
	};

	tmuctrl_0: tmuctrl_CPUCL0@10130000 {
		compatible = "samsung,exynos7570-tmu";
		reg = <0x0 0x10130000 0x200>;
		interrupts = <0 435 0>;
		default_ptat_cont = <0x1>;
	};

	tmuctrl_1: tmuctrl_G3D@10130000 {
		compatible = "samsung,exynos7570-tmu";
		reg = <0x0 0x10130000 0x200>;
		interrupts = <0 435 0>;
		default_ptat_cont = <0x1>;

		/* gpu cooling related table */
		gpu_idx_num = <5>;
		/* flags, driver_data(index), frequency */
		gpu_cooling_table = < 0 0 830000
				      0 1 666000
				      0 2 553000
				      0 3 415000
				      0 4 TABLE_END>;
	};

	pinctrl@139F0000 {
		key_volup: key-volup {
			samsung,pins = "gpa0-7";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};

		key_voldown: key-voldown {
			samsung,pins = "gpa1-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};

		if_irq: if-irq {
			samsung,pins = "gpa0-5";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};

		muic_irq: muic-irq {
			samsung,pins = "gpa1-1";
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	usi_1: usi@101D3000 {
		usi_mode = "hsi2c1";
		status = "okay";
	};

	gpio_keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&key_voldown &key_volup>;
		#address-cells = <1>;
		#size-cells = <0>;
		gpio-key,wakeup_enable;
		button@1 {
			label = "gpio-keys: KEY_VOLUMEUP";
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa0>;
			linux,code = <115>;
			gpios = <&gpa0 7 0xf>;
		};
		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			interrupts = <4 0 0>;
			interrupt-parent = <&gpa1>;
			linux,code = <114>;
			gpios = <&gpa1 4 0xf>;
		};
	};

	pinctrl@148C0000 {
		i2s_pmic_bus: i2s-pmic-bus {
			samsung,pins = "gpz1-0", "gpz1-2", "gpz1-3";
			samsung,pin-function = <2>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};

		i2s_pmic_bus_idle: i2s-pmic-bus-idle {
			samsung,pins = "gpz1-0", "gpz1-2", "gpz1-3";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@139B0000 {
		cfg_i2c: cfg-i2c {
			samsung,pins = "gpc4-1", "gpc4-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		cfg_gpio: cfg-gpio {
			samsung,pins = "gpc4-1", "gpc4-0";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
	};

	/* TSP */
	pinctrl@139D0000 {
		attn_input: attn-input {
			samsung,pins = "gpc3-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		attn_irq: attn-irq {
			samsung,pins = "gpc3-2";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		i2c3_bus: i2c3-bus {
			samsung,pins = "gpc3-1", "gpc3-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@139F0000 {
		grip_irq: tkey-irq {
			samsung,pins = "gpa0-2";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		grip_input: tkey-input {
			samsung,pins = "gpa0-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	i2c@20 {
		compatible = "i2c-gpio";
		gpios = <&gpc4 0 0
				&gpc4 1 0>;
		i2c-gpio,delay-us = <5>;
		pinctrl-names = "default","on_i2c","off_i2c";
		pinctrl-0 = <&cfg_i2c>;
		pinctrl-1 = <&cfg_i2c>;
		pinctrl-2 = <&cfg_gpio>;
		#address-cells = <1>;
		#size-cells = <0>;
		touchkey@20 {
			pinctrl-names = "default","on_irq","off_irq";
			pinctrl-0 = <&grip_irq>;
			pinctrl-1 = <&grip_irq>;
			pinctrl-2 = <&grip_input>;
			compatible = "coreriver,tc3xx-grip";
			reg = <0x20>;
			interrupts = <3 0 0>;
			interrupt-parent = <&gpa0>;
			coreriver,scl-gpio = <&gpc4 1 0>;
			coreriver,sda-gpio = <&gpc4 0 0>;
			coreriver,irq-gpio = <&gpa0 2 0>;
			coreriver,i2c-gpio = "true";
			coreriver,boot-on-ldo = "true";
			coreriver,regulator_ic = "vtouch_2.2v";
			coreriver,fw_name = "coreriver/tc305k_j2core_grip.fw";
			coreriver,sensing_ch_num = <0x4>;
			coreriver,tsk_ic_num = <0x1>; /* tc300k : 0x00, tc350k : 0x01 */
			coreriver,use_bitmap = <1>;
		};
	};

	pinctrl@139B0000 {
		sensor_i2c: sensor_i2c {
			samsung,pins = "gpc4-3", "gpc4-2";
			samsung,pin-function = <2>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <2>;
		};
	};

	pinctrl@139F0000 {
		acc_int: acc_int {
			samsung,pins = "gpa0-4";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		prox_int: prox_int {
			samsung,pins = "gpa1-0";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	i2c@13880000 {
		status = "okay";

		muic-universal@14 {
			status = "okay";
			compatible = "muic-universal";
			reg = <0x14>;
			pinctrl-names = "default";
			pinctrl-0 = <&muic_irq>;
			muic-universal,chip_name = "sm,sm5504";
			muic-universal,irq-gpio = <&gpa1 1 0>;
		};
	};

	muic {
		status = "okay";
		muic,support-list = "+OTG:GND",
					"-MHL:1K",
					"-VZW Accessory:28.7K",
					"-VZW Incompatible:34K",
					"-Smartdock:40.2K",
					"-HMT:49.9K",
					"-Audiodock:64.9K",
					"-USB LANHUB:80.07K",
					"-Charging Cable:102K",
					"-Universal Multimedia dock:121K",
					"+Jig USB Off:255K",
					"+Jig USB On:301K",
					"+Deskdock:365K",
					"+TYPE2 Charger:442K",
					"+Jig UART Off:523K",
					"+Jig UART On:619K",
					"+TA:OPEN",
					"+USB:OPEN",
					"+CDP:OPEN",
					"+DCD Timeout:OPEN",
					"+Undefined Charging:XXX";
		/*muic,otg_en = /bits/ 8 <0x1>;*/
	};

	fimc_is@14400000 {
		vender {
			rear_sensor_id = <24>; /* SENSOR_NAME_S5K4H5YC */
			front_sensor_id = <17>; /* SENSOR_NAME_S5K5E3 */
			use_module_check;

			total_camera_num = <2>;
			camera_info0 {				// 0 : rear
				isp = <0>;			/* 0 : INT , 1 : EXT , 2 : SOC */
				cal_memory = <2>;		/* 0 : N , 1 : FROM , 2 : EEPROM , 3 : OTP  */
				read_version = <0>;		/* 0 : SYSFS , 1 : CAMON */

				core_voltage = <0>;		/* 0 : N , 1 : Y  */
				upgrade = <0>;			/* 0 : N , 1 : SYSFS , 2 : CAMON */
				companion = <0>;		/* 0 : N , 1 : Y  */
				ois = <0>;			/* 0 : N , 1 : Y  */
			};
			camera_info1 {				// 1 : front
				isp = <0>;			/* 0 : INT , 1 : EXT , 2 : SOC */
				cal_memory = <3>;		/* 0 : N , 1 : FROM , 2 : EEPROM , 3 : OTP  */
				read_version = <0>;		/* 0 : SYSFS , 1 : CAMON */

				core_voltage = <0>;		/* 0 : N , 1 : Y  */
				upgrade = <0>;			/* 0 : N , 1 : SYSFS , 2 : CAMON */
				companion = <0>;		/* 0 : N , 1 : Y  */
