		"msm-dai-q6-mi2s.2", "msm-dai-q6-mi2s.3",
		"msm-dai-q6-mi2s.4",
		"msm-dai-q6-mi2s.7", "msm-dai-q6-mi2s.8",
		"msm-dai-q6-mi2s.9", "msm-dai-q6-mi2s.10",
		"msm-dai-q6-mi2s.11", "msm-dai-q6-mi2s.12",
