	det-zones-list = <&det_zones 0 2>,
			 <&det_zones 955 2>,
			 <&det_zones 956 1>,
			 <&det_zones 9999 1>;

	mbhc-button-thres = <&det_zones 13 63>,
			 <&det_zones 88 138>,
			 <&det_zones 138 138>,
			 <&det_zones 225 225>,
