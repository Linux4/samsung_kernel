			&cam_sensor_mipi_sw_suspend>;
		gpios = <&tlmm 15 0>,
			<&tlmm 82 0>,
			<&tlmm 116 0>;  //SUBCAM_SEL
		gpio-reset = <1>;
		gpio-custom1 = <2>;
		gpio-req-tbl-num = <0 1 2>;
		gpio-req-tbl-flags = <1 0 0>;
		gpio-req-tbl-label = "CAMIF_MCLK2",
					"CAM_RESET2",
					"SUBCAM_SEL";
                sensor-mode = <0>;
		cci-master = <1>;
		status = "ok";
		clocks = <&clock_camcc CAM_CC_MCLK2_CLK>;
		clock-names = "cam_clk";
