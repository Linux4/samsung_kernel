			regulator-max-microvolt = <3300000>;
			status = "okay";
		};
	};

	rpm-regulator-ldoa14 {
		pm8998_l14: regulator-l14 {
			regulator-name = "nfc_vdd";
			/delete-property/ regulator-boot-on;
