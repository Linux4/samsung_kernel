			regulator-max-microvolt = <1800000>;
			status = "okay";
		};
	};

	rpm-regulator-ldoa18 {
		pm8998_l18: regulator-l18 {
			regulator-name = "pm8998_l18_lte_u";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-boot-on;
			regulator-always-on;
			status = "okay";
		};
	};

	rpm-regulator-ldoa7 {
		pm8998_l7: regulator-l7 {
		 	regulator-name = "cam_vio";
			/delete-property/ regulator-boot-on;
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			status = "okay";
 		};
	};

	rpm-regulator-ldoa17 {
		pm8998_l17: regulator-l17 {
			regulator-name = "cam_vdig";
			/delete-property/ regulator-boot-on;
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
			status = "okay";
		};
		pm8998_l17_pin_ctrl: regulator-l17-pin-ctrl {
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;

		};
	};

	rpm-regulator-ldoa28 {
		pm8998_l28: regulator-l28 {
			regulator-name = "cam_vana";
			/delete-property/ regulator-boot-on;
			regulator-min-microvolt = <2800000>;
			regulator-max-microvolt = <2800000>;
			status = "okay";
		};
	};
};

&pm8998_l25 {
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <3600000>;
	qcom,init-voltage = <2800000>;
	status = "okay";
};
&pm8998_l21 {
	regulator-min-microvolt = <1800000>;
	regulator-max-microvolt = <1800000>;
	status = "okay";
};
&pm8998_l22 {
	regulator-min-microvolt = <3300000>;
	regulator-max-microvolt = <3300000>;
	qcom,init-voltage = <3300000>;
	status = "okay";
};

&pm8998_l23 {
	regulator-min-microvolt = <2300000>;
	regulator-max-microvolt = <3300000>;
	qcom,init-voltage = <2800000>;
	status = "okay";
};


&spmi_bus {
	qcom,pm8998@0 {
		pm8998_gpios: gpios {
			gpio@c200{ /* PM8998 GPIO 3 : BT_EN*/
				status = "okay";
				qcom,mode = <0x1>;
				qcom,pull = <0x0>;
				qcom,vin-sel = <0x0>;
				qcom,src-sel = <0x0>;
				qcom,master-en = <0x1>;
				qcom,out-strength = <0x2>;
			};

			gpio@c500 { /* PM8998 GPIO 6 VOL_UP KEY */
				status = "okay";
				qcom,mode = <0x0>;
				qcom,pull = <0x0>;
				qcom,vin-sel = <0x0>;
				qcom,src-sel = <0x0>;
				qcom,master-en = <0x1>;
				qcom,out-strength = <0x1>;
			};
			
			gpio@c600 { /* PM8998 GPIO 7 */
				qcom,pin-num = <7>;
				qcom,mode = <0>;
				qcom,pull = <0>;
				qcom,vin-sel = <0>;
				qcom,src-sel = <0>;
				qcom,master-en = <1>;
				status = "okay";
			};

			gpio@ce00 { /* PM8998 GPIO 15 HOME KEY */
				status = "okay";
				qcom,mode = <0x0>;
				qcom,pull = <0x0>;
				qcom,vin-sel = <0x0>;
				qcom,src-sel = <0x0>;
				qcom,master-en = <0x1>;
				qcom,out-strength = <0x1>;
			};

			gpio@d100 { /* PM8998 GPIO 18 */
				status = "okay";
				qcom,mode = <0x1>;	// DIG_OUT
				qcom,pull = <0x5>;	// NO_PULL
				qcom,vin-sel = <0x0>;	// VIN0
				qcom,invert = <0x0>;	// NO_INVERT
				qcom,src-sel = <0x0>;	// LOW
				qcom,master-en = <0x1>;	// GPIO_ENABLED
				qcom,out-strength = <0x1>;	// LOW_STR
			};

			gpio@d200 { /* PM8998 GPIO 19 WIN KEY*/
				status = "okay";
				qcom,mode = <0x0>;
				qcom,pull = <0x0>;
				qcom,vin-sel = <0x0>;
				qcom,src-sel = <0x0>;
				qcom,master-en = <0x1>;
				qcom,out-strength = <0x1>;
			};

			gpio@d300 { /* PM8998 GPIO 20 */
				qcom,pin-num = <20>;
				qcom,mode = <0>;
				qcom,out-strength = <0x1>;
				qcom,pull = <0>;
				qcom,vin-sel = <1>;
				qcom,src-sel = <0>;
				qcom,master-en = <1>;
				status = "okay";
			};

			gpio@d400 { /* GPIO 21 (NFC_CLK_REQ) */
				qcom,mode = <0>;
				qcom,vin-sel = <1>;
				qcom,src-sel = <0>;
				qcom,master-en = <1>;
				status = "okay";
			};
			gpio@d600 { /* GPIO 23 */
				qcom,master-en = <0>;
				status = "okay";
			};
		};
		qcom,power-on@800 {
			qcom,pon_1 {
				qcom,pon-type = <0>;
				qcom,support-reset = <0>;
				qcom,pull-up = <1>;
				linux,code = <551>;
				switch_powerkey;
			};

			qcom,pon_2 {
				qcom,pon-type = <1>;
				qcom,support-reset = <0>;
				qcom,pull-up = <1>;
				linux,code = <114>;
			};

			qcom,pon_3 {
				qcom,pon-type = <3>;
				qcom,support-reset = <1>;
				qcom,pull-up = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <1000>;
				qcom,s2-type = <8>;
				qcom,use-bark;
			};
		};
	};

	qcom,pm8005@4 {
		gpios {
			gpio@c000 {/* PM8005 GPIO 1 */
				status = "okay";
				qcom,mode = <0x0>;
				qcom,pull = <0x0>;
				qcom,vin-sel = <0x0>;
