        usb_noti: usb-notifier {
			compatible = "samsung,usb-notifier";
			qcom,disable_control_en = <1>;
	};

	sec_abc {
				compatible = "samsung,sec_abc";
				status = "okay";

				gpu {
					gpu,label="GPU fault";
					gpu,threshold_count=<20>;
					gpu,threshold_time=<1200>;
				};
				aicl {
					aicl,label="battery aicl";
					aicl,threshold_count=<5>;
					aicl,threshold_time=<300>;
				};
			};

	abc_hub {
		compatible = "samsung,abc_hub";
		status = "okay";

		bootc {
			bootc,time_spec_user = <100000>; /* user binary user build */
			bootc,time_spec_eng = <100000>; /* user binary eng build */
			bootc,time_spec_fac = <100000>; /* factory binary */
		};
	};

	/* usb speed limitation */
        usb3: ssusb@a800000 {
		dwc3@a800000 {
			maximum-speed = "high-speed";
                };
        };

	tlmm: pinctrl@03000000 {
		sdc2_clk_on: sdc2_clk_on {
				     config {
					     pins = "sdc2_clk";
					     drive-strength = <10>; /* 10 MA */
					     bias-disable; /* NO pull */
				     };
			     };

		sdc2_cmd_on: sdc2_cmd_on {
				     config {
					     pins = "sdc2_cmd";
					     bias-pull-up; /* pull up */
					     drive-strength = <8>; /* 8 MA */
				     };
			     };

		sdc2_data_on: sdc2_data_on {
				      config {
					      pins = "sdc2_data";
					      bias-pull-up; /* pull up */
					      drive-strength = <8>; /* 8 MA */
				      };
			      };
	};
};

&spmi_bus {
	qcom,pm660@1 {
		pm660_haptics: qcom,haptic@c000 {
			qcom,wave-play-rate-us = <4868>;	/* 205Hz */
			qcom,vmax-mv = <2000>;			/* 2.0v test*/
			qcom,wave-shape = "square";
                        qcom,lra-hw-auto-resonance;
	       };
	};
};

&firmware {
	android {
		fstab {
			odm {
				compatible = "android,odm";
				dev = "/dev/block/platform/soc/1da4000.ufshc/by-name/odm";
				type = "ext4";
				mnt_flags = "ro";
				fsmgr_flags = "wait,verify";
			};
			system {
				compatible = "android,system";
				dev = "/dev/block/platform/soc/1da4000.ufshc/by-name/system";
				type = "ext4";
				mnt_flags = "ro,errors=panic";
				fsmgr_flags = "wait,verify";
			};
			vendor {
				compatible = "android,vendor";
				dev = "/dev/block/platform/soc/1da4000.ufshc/by-name/vendor";
				type = "ext4";
				mnt_flags = "ro,barrier=1,discard";
				fsmgr_flags = "wait,verify";
			};
		};
	};
};

&pm660_gpios {
	/* GPIO 4 (NFC_CLK_REQ) */
	gpio@c300 {
			qcom,mode = <0>;
			qcom,vin-sel = <1>;
			qcom,src-sel = <0>;
			qcom,master-en = <1>;
			status = "okay";
	};

	/* GPIO 6 (NC) */
	gpio@c500 {
			qcom,mode = <0>;
			qcom,vin-sel = <0>;
			qcom,src-sel = <0>;
			qcom,master-en = <0>;
			status = "okay";
	};

	/* GPIO 9 (NC) */
	gpio@c800 {
			qcom,mode = <0>;
			qcom,vin-sel = <0>;
			qcom,src-sel = <0>;
			qcom,master-en = <0>;
			status = "okay";
	};

	/* GPIO 12 (WIFI_THM) */
	gpio@cb00 {
		qcom,master-en = <0>;
		status = "okay";
	};

	/* GPIO 13 (HALL IC) */
	gpio@cc00 {
			qcom,mode = <0>;
			qcom,pull = <5>;
			qcom,vin-sel = <0>;
			qcom,src-sel = <0>;
			status = "okay";
	};
};

&pm660l_gpios {
	gpio@c300 {
		qcom,mode = <1>;                /* Output */
		qcom,pull = <5>;                /* No Pull */
		qcom,vin-sel = <0>;             /* VIN1 GPIO_LV */
		qcom,src-sel = <0>;             /* GPIO */
		qcom,invert = <0>;              /* Invert */
		qcom,master-en = <1>;           /* Enable GPIO */
		status = "ok";
	};

	gpio@c200 {
		qcom,mode = <1>;                /* Output */
		qcom,pull = <5>;                /* No Pull */
		qcom,vin-sel = <0>;             /* VIN1 GPIO_LV */
		qcom,src-sel = <0>;             /* GPIO */
		qcom,invert = <0>;              /* Invert */
		qcom,master-en = <1>;           /* Enable GPIO */
		status = "ok";
	};

	/* GPIO 5, grip int */
	gpio@c400 {
		qcom,mode = <0>;
		qcom,pull = <5>;
		qcom,vin-sel = <0>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		status = "okay";
	};
	
	/* GPIO 6 for NC */
	gpio@c500 {
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <0>;
		qcom,src-sel = <0>;
		qcom,master-en = <0>;
		status = "okay";
	};

	/* GPIO 7 for KEY_VOLUMEUP */
	gpio@c600 {
		status = "okay";
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <1>;	/* PAD TYPE = MV : VIN0 (VPH_PWR), VIN1 (1.8V) */
		qcom,src-sel = <0>;
		qcom,out-strength = <1>;
	};

	/* GPIO 8 for BIXBY_KEY */
	gpio@c700 {
		status = "okay";
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <0>;	/* PAD TYPE = LV : VIN0 (1.8V), VIN1 (cannot select)  */
		qcom,src-sel = <0>;
		qcom,out-strength = <1>;
	};
};

&pm660_vadc {
	chan@4f {
		label = "pa_therm0";
		reg = <0x4f>;
		qcom,decimation = <2>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
		qcom,vadc-thermal-node;
	};

	chan@59 {
		label = "gpio12_adc";
		reg = <0x59>;				// Channel for PM660 GPIO 12
		qcom,decimation = <2>;
		qcom,pre-div-channel-scaling = <0>;	// 1:1 Scaling
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};
};

&rpm_bus {
	rpm-regulator-ldoa18 {
		compatible = "qcom,rpm-smd-regulator-resource";
		qcom,resource-name = "ldoa";
		qcom,resource-id = <18>;
		qcom,regulator-type = <0>;
		status = "okay";
	
		pm660_l18: regulator-l18 {
			compatible = "qcom,rpm-smd-regulator";
			regulator-name = "pm660_l18";
			qcom,set = <3>;
			regulator-min-microvolt = <2800000>;
			regulator-max-microvolt = <2800000>;
			status = "okay";
		};
	};
};

&sdhc_2 {
	cd-gpios = <&tlmm 54 0x0>;
	sec,tflash-en-gpio = <&expander_gpios 14 0>;     /* GPIO Expander P1_6 */
	qcom,clk-rates = <400000 20000000 25000000 50000000 100000000
		200000000>;
};
