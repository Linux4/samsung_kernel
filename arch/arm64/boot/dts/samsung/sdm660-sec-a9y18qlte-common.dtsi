/{
	aliases {
		i2c6 = &i2c_6;
		i2c7 = &i2c_7;
		i2c21 = &i2c_21;
		/* i2c22 = &i2c_22; */
		i2c23 = &i2c_23;
		i2c24 = &i2c_24;
		i2c25 = &i2c_25;
