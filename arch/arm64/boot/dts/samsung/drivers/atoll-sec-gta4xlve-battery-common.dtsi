#include <dt-bindings/iio/qcom,spmi-vadc.h>

&pm6150_vadc {
	bat_thm { /* AMUX_3 */
		label = "bat_thm";
		reg = <ADC_AMUX_THM3_PU2>;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};
	chg_thm { /* AMUX_1 */
		label = "chg_thm";
		reg = <ADC_AMUX_THM1_PU2>;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};
};

&pm6150l_gpios {
	blkt_thm { /* GPIO_07 */
		blkt_thm_default: blkt_thm_default {
			pins = "gpio7";
			function = "normal";
			bias-high-impedance;
		};
	};
};

&pm6150l_vadc {
	blkt_thm { /* GPIO_07 */
		label = "blkt_thm";
		reg = <ADC_GPIO3_PU2>;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};
};

