		battery,chg_gpio_en = <&gpd0 0 0>;
		battery,chg_polarity_en = <0>;
		battery,chg_gpio_status = <0>;
		battery,chg_polarity_status = <0>;
		battery,chg_float_voltage = <4300>;
		chg_autostop = <1>;
		chg_autoset = <1>;
		chg_aiclen = <1>;
		chg_aiclth = <4300>;
		chg_vbuslimit = <0>;
		fg_vol_val = <4340>;
		fg_soc_val = <95>;
		fg_curr_avr_val = <150>;
