	spi8_active {
		/* MOSI, MISO*/
		qcom,pins = <&gp 96>, <&gp 97>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <1>;
		label = "spi8-active";
		/* active state */
		spi8_default: spi8_default {
			drive-strength = <12>; /* 12 MA */
			bias-disable = <0>; /* No PULL */
		};
	};

	spi8_suspend {
		/* MOSI, MISO*/
		qcom,pins = <&gp 96>, <&gp 97>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <1>;
		label = "spi8-suspend";
		/* suspended state */
		spi8_sleep: spi8_sleep {
			drive-strength = <2>; /* 2 MA */
			bias-disable = <0>; /* No PULL */
		};
	};

	spi8_cs0_active {
		/* CS, CLK */
		qcom,pins = <&gp 98>, <&gp 99>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <2>;
		label = "spi8-cs0-active";

		spi8_cs0_active: cs0_active {
			drive-strength = <2>;
			bias-disable = <0>;
		};
	};

	spi8_cs0_suspend {
		/* CS, CLK */
		qcom,pins = <&gp 98>, <&gp 99>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <2>;
		label = "spi8-cs0-suspend";

		spi8_cs0_sleep: cs0_sleep {
			drive-strength = <2>;
			bias-disable = <0>;
		};
	};

	hall_int {
                qcom,pins = <&gp 48>; /* INTERRUPT */
                qcom,num-grp-pins = <1>;
                qcom,pin-func = <0>;
                label = "hall_int";
                hall_int_active: hall_int_active {
			drive-strength = <2>;
                        bias-disable;
                };
                hall_int_suspend: hall_int_suspend {
			drive-strength = <2>;
                        bias-disable;
                };
        };

	pmx_mdss: pmx_mdss {			// GPIO 41 & 61
		label = "mdss-pins";
		qcom,pin-func = <0>;
		mdss_dsi_active: active {
			drive-strength = <8>;
			bias-disable = <0>;	// no pull
		};
		mdss_dsi_suspend: suspend {
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	pmx_mdss_te: pmx_mdss_te {		// GPIO 24
		label = "mdss-te-pin";
		qcom,pin-func = <1>;
		mdss_te_active: active {
			drive-strength = <2>;
			bias-pull-down;
			input-debounce = <0>;
		};
		mdss_te_suspend: suspend {
			drive-strength = <2>;
			bias-pull-down;
			input-debounce = <0>;
		};
	};

	pmx_mdss_esd: pmx_mdss_esd {		// GPIO 13 & 54
		label = "mdss-esd-pin";
		qcom,pin-func = <0>;
		mdss_esd_active: active {
			drive-strength = <2>;
			bias-disable = <0>;	// no pull
		};
		mdss_esd_suspend: suspend {
			drive-strength = <2>;
			bias-disable = <0>;	// no pull
		};
	};
};

&soc {
	tlmm_pinmux: pinctrl@1000000 {
		compatible = "qcom,msm-tlmm-8916";
		reg = <0x1000000 0x300000>;
		interrupts = <0 208 0>;

		/* cci */
		cci0_active {
			/* CLK, DATA */
			qcom,pins = <&gp 29>, <&gp 30>;
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <1>;
			label = "cci0-active";
			/* active state */
			cci0_active: cci0_active {
				drive-strength = <2>; /* 2 MA */
				bias-disable = <0>; /* No PULL */
			};
		};

		cci0_suspend {
			/* CLK, DATA */
			qcom,pins = <&gp 29>, <&gp 30>;
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <0>;
			label = "cci0-suspend";
			/*suspended state */
			cci0_suspend: cci0_suspend {
				drive-strength = <2>; /* 2 MA */
				bias-disable = <0>; /* No PULL */
			};
		};

		cci1_active {
			/* CLK, DATA */
			qcom,pins = <&gp 31>, <&gp 32>;
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <1>;
			label = "cci1-active";
			/* active state */
			cci1_active: cci1_active {
				drive-strength = <2>; /* 2 MA */
				bias-disable=<0>; /* No PULL */
			};
		};

		cci1_suspend {
			/* CLK, DATA */
			qcom,pins = <&gp 31>, <&gp 32>;
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <0>;
			label = "cci1-suspend";
			/*suspended state */
			cci1_suspend: cci1_suspend {
				drive-strength = <2>; /* 2 MA */
				bias-disable=<0>; /* No PULL */
			};
		};

		vfsspi_drdypin: vfsspi_drdypin {
			qcom,pins = <&gp 37>;
			qcom,pin-func = <0>;
			qcom,num-grp-pins = <1>;
			label = "vfsspi_drdypin";
			vfsspi_drdypin_active: vfsspi_drdypin_active {
				drive-strength = <2>;
				bias-pull-down;
			};
			vfsspi_drdypin_suspend: vfsspi_drdypin_suspend {
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		etspi_drdypin: etspi_drdypin {
			qcom,pins = <&gp 37>;
			qcom,pin-func = <0>;
			qcom,num-grp-pins = <1>;
			label = "etspi_drdypin";
			etspi_drdypin_active: etspi_drdypin_active {
				drive-strength = <2>;
				bias-pull-down;
			};
			etspi_drdypin_suspend: etspi_drdypin_suspend {
				drive-strength = <2>;
				bias-pull-down;
			};
		};
	};
};
