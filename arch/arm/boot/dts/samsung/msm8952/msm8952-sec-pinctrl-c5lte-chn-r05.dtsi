			output-low;
			bias-pull-down = <0>;
		};
	};

	cam_sensor_front_ctrl {
		qcom,pins = <&gp 49>,<&gp 74>;
		qcom,num-grp-pins = <2>;
		label = "cam_sensor_front_ctrl";
		cam_sensor_front_ctrl_active: cam_sensor_front_ctrl_active {
			drive-strength = <2>;
			bias-disable = <0>;
		};
		cam_sensor_front_ctrl_sleep: cam_sensor_front_ctrl_sleep {
			drive-strength = <2>;
			bias-disable = <0>; 
		};
	};
	
	quin-tlmm-lines-1 {
		qcom,pins = <&gp 92>, <&gp 93>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <3>;
		label = "quin-lines-1";
		quin_tlmm_lines1_act: quin_tlmm_lines1_on {
			drive-strength = <8>;
