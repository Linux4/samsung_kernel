	/delete-node/ tpiu_seta_18;
	/delete-node/ pri-tlmm-lines;

	earjack_det_gpio {
		  qcom,pins = <&gp 86>;
		  qcom,pin-func = <0>;
		  qcom,num-grp-pins = <1>;
		  label = "earjack_det_gpio";
		  earjack_det_act: earjack_det_act {
				drive-strength = <2>;
				bias-disable;
		  };
		  earjack_det_sus: earjack_det_sus {
				drive-strength = <2>;
				bias-disable;
		  };
	};

	earjack_swtich_gpio {
		  qcom,pins = <&gp 38>;
		  qcom,pin-func = <0>;
		  qcom,num-grp-pins = <1>;
		  label = "earjack_swtich_gpio";
		  earjack_switch_act: earjack_switch_act {
				drive-strength = <2>;
				bias-disable;
		  };
		  earjack_switch_sus: earjack_switch_sus {
				drive-strength = <2>;
				bias-disable;
		  };
	};

	/delete-node/ pmx_rd_nfc_int;
	nfc_int {
		qcom,pins = <&gp 17>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "pmx_nfc_int";

		nfc_int_active: active {
			drive-strength = <6>;
			bias-pull-down;
		};

		nfc_int_suspend: suspend {
			drive-strength = <6>;
			bias-pull-down;
		};
	};

	nfc_firmware {
		qcom,pins = <&gp 16>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "nfc_firmware";

		nfc_firmware_active: nfc_firmware_active {
			drive-strength = <2>;
			bias-pull-down; /* PULL DOWN */
		};

		nfc_firmware_suspend: nfc_firmware_suspend {
			drive-strength = <2>;
			bias-pull-down; /* PULL DOWN */
