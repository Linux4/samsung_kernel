	rt5659-ldo {
		qcom,pins = <&gp 39>;
		qcom,num-grp-pins = <1>;
		qcom,pin-func = <0>;
		label = "rt5659-ldo";
		rt5659_ldo_active: rt5659_ldo_active {
			drive-strength = <2>;
			bias-disable;
		};
		rt5659_ldo_suspend: rt5659_ldo_suspend {
			drive-strength = <2>;
			bias-disable;
		};
	};	
	
	quin-tlmm-lines-0 {
		qcom,pins = <&gp 88>, <&gp 91>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <1>;
		label = "quin-lines-0";
		quin_tlmm_lines0_act: quin_tlmm_lines_on {
			drive-strength = <8>;
			bias-pull-none;
		};
		quin_tlmm_lines0_sus: quin_tlmm_lines_off {
			drive-strength = <2>;
			bias-pull-none;
		};
	};
	
	quat-tlmm-lines-0 {
		qcom,pins = <&gp 13>, <&gp 8>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <3>;
		label = "quat-lines-0";
		quat_tlmm_lines0_act: quat_tlmm_lines_on {
			drive-strength = <8>;
			bias-disable;
		};
		quat_tlmm_lines0_sus: quat_tlmm_lines_off {
			drive-strength = <2>;
			bias-disable;
		};
	};
 
	quat-tlmm-lines-1 {
		qcom,pins = <&gp 9>, <&gp 12>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <3>;
		label = "quat-lines-1";
		quat_tlmm_lines1_act: quat_tlmm_lines1_on {
			drive-strength = <8>;
			bias-disable;
		};
		quat_tlmm_lines1_sus: quat_tlmm_lines1_off {
			drive-strength = <2>;
			bias-disable;
		};
	};


	cam_mclk0 {
		qcom,pins = <&gp 26>;
		qcom,num-grp-pins = <1>;
		qcom,pin-func = <1>;
		label = "cam_mclk0";
		cam_mclk0_active: cam_mclk0_active {
			drive-strength = <4>;
			output-low;
			bias-pull-down=<0>;
		};
		cam_mclk0_sleep: cam_mclk0_sleep {
			drive-strength = <4>;
			output-low;
			bias-pull-down=<0>;
		};
	};


	cam_flash {
		qcom,pins = <&gp 33>, <&gp 34>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <0>;
		label = "cam_flash";

		cam_flash_active: default {
			drive-strength = <2>;	/* 2 MA */
			bias-pull-down = <0>; /* PULL DOWN */
			output-low;
		};
		cam_flash_sleep: sleep {
			drive-strength = <2>;	/* 2 MA */
			bias-pull-down = <0>; /* PULL DOWN */
			output-low;
		};
	};

      cam_sensor_rear_ctrl {
		qcom,pins = <&gp 69>,<&gp 86>;
		qcom,num-grp-pins = <2>;
		label = "cam_sensor_rear_ctrl";
		cam_sensor_rear_ctrl_active: cam_sensor_rear_ctrl_active {
			drive-strength = <2>;
			bias-disable = <0>;
		};
		cam_sensor_rear_ctrl_sleep: cam_sensor_rear_ctrl_sleep {
			drive-strength = <2>;
			bias-pull-down = <0>;
		};
	};
	
	cam_mclk1 {
		qcom,pins = <&gp 28>;
		qcom,num-grp-pins = <1>;
		qcom,pin-func = <1>;
		label = "cam_mclk1";
		cam_mclk1_active: cam_mclk1_active {
			drive-strength = <4>;
			output-low;
			bias-pull-down = <0>;
		};
		cam_mclk1_sleep: cam_mclk1_sleep {
