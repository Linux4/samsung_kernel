#include "sdm450-sec-j8plte_audio-pinctrl.dtsi"

&soc {
	tlmm: pinctrl@1000000 {
		compatible = "qcom,msm8953-pinctrl";
		reg = <0x1000000 0x300000>;
		interrupts = <0 208 0>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;

		pmov_nc_pins {
			label = "pmov_nc_pins";
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			nc_gpio_default: default {
				mux {
					pins = "gpio126";
					function ="gpio";
				};
				config {
					pins = "gpio126";
					drive-strength = <2>; /* 2mA */
					bias-pull-down;
					input-enable;
				};
			};
		};

		radio_lna_en {
				lna_en_default: lna_en_default {
					mux {
						pins = "gpio134";
						function = "gpio";
					};

					config {
						pins = "gpio134";
						drive-strength = <2>;
						bias-disable;
					};
				};
				lna_en_sleep: lna_en_sleep {
					mux {
						pins = "gpio134";
						function = "gpio";
					};

					config {
						pins = "gpio134";
						drive-strength = <2>;
						bias-disable;
					};
				};
		};

		i2c_20 {
			i2c_20_active: i2c_20_active {
				mux {
					pins = "gpio20", "gpio21";
					function = "gpio";
				};
				config {
					pins = "gpio20", "gpio21";
					drive-strength = <2>;
					bias-disable;
				};
			};
		};

		expander_reset_active: expander_reset_active {
			mux {
				pins = "gpio33";
				function = "gpio";
			};
			config {
				pins = "gpio33";
				drive-strength = <2>;   /* 16 mA */
				bias-disable;
				output-high;
			};
		};

		vibrator_enable{
			vibrator_enable_active: vibrator_enable_active {
				mux {
					pins = "gpio132";
					function = "gpio";
				};

				config {
					pins = "gpio132";
					drive-strength = <2>;
					output-high;
					bias-pull-down;
				};
			};

			vibrator_enable_suspend: vibrator_enable_suspend {
				mux {
					pins = "gpio132";
					function = "gpio";
				};

				config {
					pins = "gpio132";
					drive-strength = <2>;
					bias-disable;
					output-low;
				};
			};
		};

	/* MUIC - SM5708 */
		sm5708_i2c_pins {
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <0>;
			label = "sm5708_gpio";
			sm5708_i2c_default: default {
				mux {
					pins = "gpio98", "gpio99";
					function = "gpio";
				};
				config {
					pins = "gpio98", "gpio99";
					drive-strength = <2>;
					bias-disable;
				};
			};
			sm5708_i2c_active: active {
				mux {
					pins = "gpio98", "gpio99";
					function = "gpio";
				};
				config {
					pins = "gpio98", "gpio99";
					drive-strength = <2>;
					bias-disable;
				};

			};
			sm5708_i2c_suspend: suspend {
				mux {
					pins = "gpio98", "gpio99";
					function = "gpio";
				};
				config {
					pins = "gpio98", "gpio99";
					drive-strength = <2>;
					bias-disable;
				};
			};
		};

		sm5708_intr_pins {
			qcom,pin-func = <0>;
			label = "sm5708_gpio_int";
			sm5708_interrupt_default: default {
				mux {
					pins = "gpio63";
					function = "gpio";
				};
				config {
					pins = "gpio63";
					drive-strength = <2>;
					bias-disable;
				};
			};
			sm5708_interrupt_active: active {
				mux {
					pins = "gpio63";
					function = "gpio";
				};
				config {
					pins = "gpio63";
					drive-strength = <2>;
					bias-disable;
				};
			};
			sm5708_interrupt_suspend: suspend {
				mux {
					pins = "gpio63";
					function = "gpio";
				};
				config {
					pins = "gpio63";
					drive-strength = <2>;
					bias-disable;
				};
			};
		};

		sm5708_rst_pins {
			label = "sm5708_rst_pin";
			sm5708_rst_default: default {
				mux {
					pins = "gpio61";
					function = "gpio";
				};
				config {
					pins = "gpio61";
					drive-strength = <2>;
					bias-disable;
					input-enable;
				};
			};
		};

		charger_int_pin {
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "charger_int_gpio";
			charger_int_active: charger_int_active {
				mux {
					pins = "gpio60";
					function = "gpio";
				};
				config {
					pins = "gpio60";
					drive-strength = <2>;
					bias-disable;         /* No PULL */
				};
			};
		};

		chg_en_pin {
			pins = "gpio34";
			qcom,num-grp-pins = <1>;	
			qcom,pin-func = <0>;
			label = "chg_en_gpio";
			chg_en_default: chg_en_default {
				mux {
					pins = "gpio34";
					function = "gpio";
				};
				config {
					pins = "gpio34";
					drive-strength = <2>;
					bias-pull-down;
					output-high;
				};
			};
		};

		fuel_int_pin {
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "fuel_int_gpio";

			fuel_int_default: fuel_int_default {
