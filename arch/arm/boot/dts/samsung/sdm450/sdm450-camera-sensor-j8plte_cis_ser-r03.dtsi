&soc {	
	led_flash0: qcom,camera-flash {
		cell-index = <0>;
		compatible = "qcom,camera-flash";
		qcom,flash-type = <3>;
		qcom,cci-master = <0>;
		qcom,flash-name = "sm5708";
		qcom,flash-operation-type = "mono";
	};
};

&i2c_3 {
    status = "okay";
    actuator0: qcom,actuator@0 {
		compatible = "qcom,actuator";
		cell-index = <0>;
		qcom,actuator-name = "zc535";
		reg = <0x18>;
		qcom,slave-addr = <0x18>;

		gpios = <&tlmm 92 0>,
		    <&tlmm 89 0>,
		    <&expander_gpios 12 0>;
		qcom,gpio-vio = <0>;		
		qcom,gpio-vaf = <1>;
		qcom,gpio-custom1 = <2>;
		qcom,gpio-req-tbl-num = <0 1 2>;
		qcom,gpio-req-tbl-flags = <0 0 0>;
		qcom,gpio-req-tbl-label = "CAM_VIO","CAM_VAF", "CAM_CUSTOM1";
        qcom,sensor-position = <0>;
		qcom,cam-power-seq-type = "sensor_gpio", "sensor_gpio", "sensor_gpio";
		qcom,cam-power-seq-val = "sensor_gpio_vio", "sensor_gpio_vaf", "sensor_gpio_custom1";
		qcom,cam-power-seq-cfg-val = <1 1 1>;
		qcom,cam-power-seq-delay = <2 2 2>;
