&soc {
	i2c_13: i2c@13 {
		cell-index = <13>;
		compatible = "i2c-gpio";
		gpios = <&tlmm 73 0 /* sda */
			&tlmm 74 0	/* scl */
			>;
		i2c-gpio,delay-us = <2>;
		#address-cells = <1>;
		#size-cells = <0>;

		muic-sm5705@25 {
			status = "okay";
			compatible = "muic-universal";
			reg = <0x25>;
			interrupt-parent = <&tlmm>;
			interrupts = <63 0>;
			muic-universal,irq-gpio = <&tlmm 63 0x00>;
			sm5705,wakeup;
			muic-universal,chip_name = "sm,sm5705";
			pinctrl-names = "muic_i2c_pins_default",
				"muic_i2c_pins_active",
				"muic_i2c_pins_suspend";
			pinctrl-0 = <&sm5705_i2c_default &sm5705_interrupt_default>;
			pinctrl-1 = <&sm5705_i2c_active &sm5705_interrupt_default>;
			pinctrl-2 = <&sm5705_i2c_suspend &sm5705_interrupt_default>;
			muic,sm5705_switch_gpio = <&tlmm 45 0>;
			muic,max-afc-support-volt = <4>;
			muic,max-afc-support-cur = <8>;
		};

		sm5705@49{
			compatible = "sm,sm5705";
			reg = <0x49>;
			interrupt-parent = <&tlmm>;
			sm5705,irq-gpio = <&tlmm 61 0x00>;
			pinctrl-names = "default";
			pinctrl-0 = <&charger_int_active &charger_en_active>;
//			sm5705,mrstb-gpio = <&tlmm 87 0x00>;
/*
			sm5705_charger {
				compatible = "siliconmitus,sm5705-charger";
				chg_autostop = <0>;
				chg_autoset = <1>;
				chg_aiclen = <1>;
				chg_aiclth = <4300>;
				fg_vol_val = <4350>;
				fg_soc_val = <95>;
				fg_curr_avr_val = <150>;
			};
*/
		};
		sm5705_fled {
			compatible = "siliconmitus,sm5705-fled";
			enable = <1 1>;
			sm5705_fled@0 {
				id=<0>;
				status = "okay";
				flash-mode-current-mA = <1100>;
				preflash-mode-current-mA = <150>;
				torch-mode-current-mA = <50>;
				used-gpio-control = <1>;
				flash-en-gpio = <&tlmm 68 0x0>;
				torch-en-gpio = <&expander_gpios 0 0>;
			};
			sm5705_fled@1 {
				id=<1>;
				status = "okay";
				flash-mode-current-mA = <300>;
				preflash-mode-current-mA = <50>;
				torch-mode-current-mA = <30>;
				used-gpio-control = <1>;
				flash-en-gpio = <&tlmm 69 0x0>;
				torch-en-gpio = <&expander_gpios 2 0>;
			};
			sm5705_leds1: sm5705-leds1 {
				ledname = "leds-sec1";
				/* sm5705_FLASH_LED_1 */
				id = <0>;
				/* sm5705_FLASH_CURRENT */
				brightness = <0xC>;
				/* sm5705_FLASH_TIMEOUT */
				timeout = <0x3>;
				default-trigger = "flash_trigger";
			};

			sm5705_leds2: sm5705-leds2 {
				ledname = "torch-sec1";
				/* sm5705_TORCH_LED_1 */
				id = <1>;
				/* sm5705_TORCH_CURRENT */
				brightness = <0xC>;
				/* sm5705_TORCH_TIMEOUT */
				timeout = <0xF>;
				default-trigger = "torch_trigger";
			};
		};
	};
};

/ {
	muic {
		status = "okay";
		muic,support-list = "+OTG:GND",
					"-MHL:1K",
					"-VZW Accessory:28.7K",
					"-VZW Incompatible:34K",
					"-Smartdock:40.2K",
					"-HMT:49.9K",
					"-Audiodock:64.9K",
					"-USB LANHUB:80.07K",
					"-Charging Cable:102K",
					"-Universal Multimedia dock:121K",
					"+Jig USB Off:255K",
					"+Jig USB On:301K",
					"+Deskdock:365K",
					"+TYPE2 Charger:442K",
					"+Jig UART Off:523K",
					"+Jig UART On:619K",
					"+TA:OPEN",
					"+USB:OPEN",
					"+CDP:OPEN",
					"+Undefined Charging:XXX";
	};
};

