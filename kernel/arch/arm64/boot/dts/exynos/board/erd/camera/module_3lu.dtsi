						80 { pname = "VDDD_0.92V_VT"; pin = <PIN_REGULATOR 0 0>; };
						90 { pname = "VDDA_2.2V_VT"; pin = <PIN_REGULATOR 0 0>; };
						100 { pname = "VDDIO_1.8V_COMMON"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>; };
