						vc1 = </* in */ 2 HW_FORMAT_UNKNOWN DATA_NONE 0 0		/* out */ HW_FORMAT_UNKNOWN VC_NOTHING 0 0>;
						vc2 = </* in */ 1 HW_FORMAT_RAW10_POTF DATA_HPD 4000 562	/* out */ HW_FORMAT_RAW10_POTF VC_TAILPDAF 4000 562>;
						vc3 = </* in */ 3 HW_FORMAT_UNKNOWN DATA_NONE 0 0		/* out */ HW_FORMAT_UNKNOWN VC_NOTHING 0 0>;
						option { votf = <0>; };
					};
					mode4_aeb { /* SENSOR_3LU_4000X2252_60FPS_10BIT = 4 */ /* A-09 */ /* AEB */
						common = <4000 2252 60 0 4 CSI_DATA_LANES_3 1862 CSI_MODE_VC_DT LRTE_ENABLE PD_MOD3 EX_AEB>;
						vc0 = </* in */ 0 HW_FORMAT_RAW10 DATA_IMG 4000 2252	/* out */ HW_FORMAT_RAW10 VC_NOTHING 4000 2252>;
						vc1 = </* in */ 2 HW_FORMAT_RAW10 DATA_IMG 4000 2252	/* out */ HW_FORMAT_RAW10 VC_NOTHING 4000 2252>;
						vc2 = </* in */ 1 HW_FORMAT_RAW10_POTF DATA_HPD 4000 562	/* out */ HW_FORMAT_RAW10_POTF VC_TAILPDAF 4000 562>;
						vc3 = </* in */ 3 HW_FORMAT_RAW10_POTF DATA_HPD 4000 562	/* out */ HW_FORMAT_UNKNOWN VC_NOTHING 0 0>;
						option { votf = <0>; max_fps = <60>; };
					};
					mode5 { /* SENSOR_3LU_4000X2252_60FPS_ADC = 5 */ /* A-12 */
						common = <4000 2252 60 0 5 CSI_DATA_LANES_3 1862 CSI_MODE_VC_DT LRTE_ENABLE PD_MOD3>;
						vc0 = </* in */ 0 HW_FORMAT_RAW12 DATA_IMG 4000 2252	/* out */ HW_FORMAT_RAW12 VC_NOTHING 4000 2252>;
						vc1 = </* in */ 2 HW_FORMAT_UNKNOWN DATA_NONE 0 0		/* out */ HW_FORMAT_UNKNOWN VC_NOTHING 0 0>;
						vc2 = </* in */ 1 HW_FORMAT_RAW10_POTF DATA_HPD 4000 562	/* out */ HW_FORMAT_RAW10_POTF VC_TAILPDAF 4000 562>;
						vc3 = </* in */ 3 HW_FORMAT_UNKNOWN DATA_NONE 0 0		/* out */ HW_FORMAT_UNKNOWN VC_NOTHING 0 0>;
