&abox {
	samsung,conf-file-name = "abox_solution_ti.conf", "abox_system.conf";
};

&hsi2c_16 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	clock-frequency = <400000>;

	tfa98xx@34 {
		status = "disabled";
	};

	tfa98xx@35 {
		status = "disabled";
	};

	tas25xx: tas25xx@48 {
		pinctrl-names = "default";
		pinctrl-0 = <&rcv_amp_en &spk_amp_en &spk_amp_int>;
		#sound-dai-cells = <1>;
		compatible = "ti,tas25xx";
		reg = <0x48>;
		ti,max-channels = <2>;
		ti,channel-0 = <0x49>;
		ti,channel-1 = <0x48>;
		ti,reset-gpio = <&gpm8 0 0>;
		ti,irq-gpio = <&gpm9 0 0>;
		ti,reset-gpio2 = <&gpm5 0 0>;
		status = "okay";
	};

};


&hsi2c_28 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "ok";
	clock-frequency = <400000>;
	samsung,reset-before-trans;
	samsung,stop-after-trans;	/* LSI IC ONLY */
};

&udc {
	status = "okay";
};

&usbdrd_dwc3 {
	status = "okay";
	maximum-speed = "high-speed";
	usb_audio_offloading = <1>;
	abox = <&abox>;
	samsung,abox-gic = <&abox_gic>;
	xhci_use_uram_for_audio = <1>;

	vdd12_usb-supply = <&l23_reg>;
	vdd18_usb-supply = <&l4_reg>;
	vdd33_usb-supply = <&l6_reg>;
};

&usbdrd_phy0 {
	status = "okay";
	hs_tune_param = <&usb_hs_tune>;

	low_power_call = <1>;

	vdd12_usb-supply = <&l23_reg>;
	vdd18_usb-supply = <&l4_reg>;
	vdd33_usb-supply = <&l6_reg>;
};

&pinctrl_0 {
	key_volup: key-volup {
		samsung,pins = "gpa0-3";
		samsung,pin-function = <0xf>;
		samsung,pin-pud = <3>;
		samsung,pin-drv = <3>;
	};
	attn_irq: attn-irq {
		samsung,pins = "gpc10-0";
		samsung,pin-function = <0xf>;
		samsung,pin-pud = <3>;
		samsung,pin-drv = <3>;
	};

	attn_input: attn-input {
		samsung,pins = "gpc10-0";
		samsung,pin-function = <0>;
		samsung,pin-pud = <1>;
	};

	dc_irq: dc-irq {
		samsung,pins = "gpa0-6";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};
};

&drmdsim_0 {
	#address-cells = <1>;
	#size-cells = <0>;
	dsi-burst-cmd = <1>;
	drmpanel {
		compatible = "samsung,mcd-panel-samsung-drv";
		/* reset, power */
		reset-gpios = <&gpc5 0 GPIO_ACTIVE_HIGH>;
		reg = <0>;
	};
};

/* RCD */
&drmdpp12 {
	/* RCD BLOCK mode configurations, based on panel size*/
	rcd_block_mode_en = <1>;

	/* block mode start offset (x,y) */
	rcd_block_mode_x  = <0>;
	rcd_block_mode_y  = <128>;

	/* block mode width & height from offset (x,y) */
	rcd_block_mode_w  = <1080>;
	rcd_block_mode_h  = <2084>;
};

&dwmmc_2 {
	card-detect-invert;
	sec-sd-slot-type = <3>; /* Hybrid Tray SD slot */
};

&ufs {
	vcc-supply = <&l29_reg>;
